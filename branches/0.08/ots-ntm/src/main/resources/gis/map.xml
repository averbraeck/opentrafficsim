<?xml version="1.0" encoding="UTF-8" ?>
<!-- Created by Alexander Verbraeck on Sep 9, 2014 -->
<gisbeans:map xmlns:gisbeans="http://www.javel.nl/gisbeans">

	<!-- We start with the global map attributes -->
	<name>Netherlands</name>
	<units>dd</units>
	<extent>
		<minX>65000.0</minX>
		<minY>440000.0</minY>
		<maxX>55000.0</maxX>
		<maxY>30000.0</maxY>
	</extent>
	
	<gisbeans:image>
		<backgroundColor>
			<r>255</r>
			<g>255</g>
			<b>255</b>
		</backgroundColor>
		<size>
			<height>2000</height>
			<width>2000</width>
		</size>
	</gisbeans:image>

	<!-- Now we get the layers -->
	<gisbeans:layer>
		<name>areas</name>
		<data>
			<shapeFile>/gis/areas.shp</shapeFile>
		</data>
		<color>
			<r>255</r>
			<g>255</g>
			<b>255</b>
		</color>
		<outlineColor>
			<r>200</r>
			<g>200</g>
			<b>200</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	
	<gisbeans:layer>
		<name>links</name>
		<data>
			<shapeFile>/gis/links.shp</shapeFile>
		</data>
		<color>
			<r>255</r>
			<g>0</g>
			<b>0</b>
		</color>
		<outlineColor>
			<r>255</r>
			<g>0</g>
			<b>0</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	
	<gisbeans:layer>
		<name>nodes</name>
		<data>
			<shapeFile>/gis/nodes.shp</shapeFile>
		</data>
		<color>
			<r>0</r>
			<g>0</g>
			<b>0</b>
		</color>
		<outlineColor>
			<r>0</r>
			<g>0</g>
			<b>0</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	
	<gisbeans:layer>
		<name>waterways</name>
		<data>
			<shapeFile>/gis/centroids.shp</shapeFile>
		</data>
		<color>
			<r>0</r>
			<g>0</g>
			<b>255</b>
		</color>
		<outlineColor>
			<r>0</r>
			<g>0</g>
			<b>255</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	
	<gisbeans:layer>
		<name>counts</name>
		<data>
			<shapeFile>/gis/counts.shp</shapeFile>
		</data>
		<color>
			<r>0</r>
			<g>127</g>
			<b>0</b>
		</color>
		<outlineColor>
			<r>0</r>
			<g>127</g>
			<b>0</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
</gisbeans:map>
