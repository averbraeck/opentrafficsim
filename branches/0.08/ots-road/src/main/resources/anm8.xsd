                <xs:restriction base="xs:string">
                        <xs:enumeration value="FixedTime"/>
                        <xs:enumeration value="Nema"/>
                        <xs:enumeration value="VAP"/>
                        <xs:enumeration value="SiemensVA"/>
                        <xs:enumeration value="Extern"/>
                        <xs:enumeration value="VsPlus"/>
                        <xs:enumeration value="RBC"/>
                        <xs:enumeration value="ASC3"/>
                        <xs:enumeration value="D4"/>
                        <xs:enumeration value="2033"/>
                </xs:restriction>
        </xs:simpleType>
        <xs:simpleType name="XmlAnmSignalType">
                <xs:annotation>
                        <xs:documentation>Contains all possible signal types.</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string">
                        <xs:enumeration value="Circular"/>
                        <xs:enumeration value="RightArrow"/>
                        <xs:enumeration value="LeftArrow"/>
                </xs:restriction>
        </xs:simpleType>
        <xs:simpleType name="XmlAnmSignType">
                <xs:annotation>
                        <xs:documentation>Contains all possible sign types.</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string">
                        <xs:enumeration value="Nothing"/>
                        <xs:enumeration value="Yield"/>
                        <xs:enumeration value="Stop"/>
                </xs:restriction>
        </xs:simpleType>
        <xs:simpleType name="XmlAnmDetectorType">
                <xs:annotation>
                        <xs:documentation>Contains all possible detector types.</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string">
                        <xs:enumeration value="Standard"/>
                        <xs:enumeration value="CallingPoint"/>
                </xs:restriction>
        </xs:simpleType>
        <xs:complexType name="XmlAnmAffectedPTLine">
                <xs:annotation>
                        <xs:documentation>A PT line which is affected for a specific network element.</xs:documentation>
                </xs:annotation>
                <xs:attribute name="ID " type="xs:string" use="required"/>
        </xs:complexType>
        <xs:complexType name="XmlAnmAffectedPTLines">
                <xs:annotation>
                        <xs:documentation>A list with affected PT lines.</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                        <xs:element name="AFFECTEDPTLINE" type="XmlAnmAffectedPTLine" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
        </xs:complexType>
        <xs:complexType name="XmlAnmDetector">
                <xs:annotation>
                        <xs:documentation>A detector that collects traffic data.</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                        <xs:element name="AFFECTEDPTLINES" type="XmlAnmAffectedPTLines" minOccurs="0"/>
                        <xs:element name="AFFECTEDVEHCLASSES" type="XmlAnmAffectedVehClasses" minOccurs="0">
                                <xs:annotation>
                                        <xs:documentation>Contains all vehicle classes that are detected.</xs:documentation>
                                </xs:annotation>
                        </xs:element>
                </xs:sequence>
                <xs:attribute name="TYPE" type="XmlAnmDetectorType" use="optional">
