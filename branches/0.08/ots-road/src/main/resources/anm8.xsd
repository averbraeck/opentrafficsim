                <xs:attribute name="VOLUME" type="xs:double" use="required"/>
        </xs:complexType>
        <xs:complexType name="XmlAnmMatrices">
                <xs:annotation>
                        <xs:documentation>A list containing matrices.</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                        <xs:element name="MATRIX" type="XmlAnmMatrix" minOccurs="0" maxOccurs="unbounded"/>
