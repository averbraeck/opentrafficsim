        <xs:simpleType name="XmlAnmSignalGroupType">
                <xs:annotation>
                        <xs:documentation>Contains all possible signal group types.</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string">
                        <xs:enumeration value="Cycle"/>
                        <xs:enumeration value="PermanentGreen"/>
                        <xs:enumeration value="PermanentRed"/>
                        <xs:enumeration value="GreenArrow"/>
                        <xs:enumeration value="Flashing"/>
                </xs:restriction>
        </xs:simpleType>
        <xs:simpleType name="XmlAnmVehicleCategoryType">
                <xs:annotation>
                        <xs:documentation>Contains all possible vehicle category types.</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string">
                        <xs:enumeration value="Tram"/>
                        <xs:enumeration value="Bus"/>
                        <xs:enumeration value="Pedestrian"/>
                        <xs:enumeration value="HGV"/>
                        <xs:enumeration value="Car"/>
                        <xs:enumeration value="Bike"/>
                </xs:restriction>
        </xs:simpleType>
        <xs:simpleType name="XmlAnmDrivingBehaviorType">
                <xs:annotation>
                        <xs:documentation>Contains all possible driving behavior types.</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string">
                        <xs:enumeration value="Urban"/>
                        <xs:enumeration value="RightSideRule"/>
                        <xs:enumeration value="Freeway"/>
                        <xs:enumeration value="Footpath"/>
                        <xs:enumeration value="CyclePath"/>
                </xs:restriction>
        </xs:simpleType>
        <xs:simpleType name="XmlAnmControlType">
                <xs:annotation>
                        <xs:documentation>Contains all possible control types.</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string">
                        <xs:enumeration value="Unknown"/>
                        <xs:enumeration value="Uncontrolled"/>
                        <xs:enumeration value="TwoWayStop"/>
                        <xs:enumeration value="TwoWayYield"/>
                        <xs:enumeration value="Signalized"/>
                        <xs:enumeration value="AllWayStop"/>
                        <xs:enumeration value="Roundabout"/>
                </xs:restriction>
        </xs:simpleType>
        <xs:simpleType name="XmlAnmSignalizationType">
                <xs:annotation>
