<?xml version="1.0" encoding="UTF-8"?>
<ots:Ots xmlns:ots="http://www.opentrafficsim.org/ots" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opentrafficsim.org/ots ../../../../ots-parser-xml/src/main/resources/xsd/ots.xsd">
  <ots:Definitions>
    <ots:GtuTypes>
      <ots:GtuType Id="CAR"/>
    </ots:GtuTypes>
    <ots:GtuTemplates>
      <ots:GtuTemplate GtuType="CAR" Id="CAR">
        <ots:LengthDist LengthUnit="m">
          <ots:Uniform Max="5" Min="3.5"/>
        </ots:LengthDist>
        <ots:WidthDist LengthUnit="m">
          <ots:Uniform Max="2.1" Min="1.7"/>
        </ots:WidthDist>
        <ots:MaxSpeedDist SpeedUnit="km/h">
          <ots:Constant C="210"/>
        </ots:MaxSpeedDist>
      </ots:GtuTemplate>
    </ots:GtuTemplates>
    <ots:LinkTypes>
      <ots:LinkType Id="URBAN">
        <ots:Compatibility GtuType="CAR"/>
        <ots:SpeedLimit GtuType="CAR" LegalSpeedLimit="50km/h"/>
      </ots:LinkType>
    </ots:LinkTypes>
    <ots:LaneTypes>
      <ots:LaneType Id="URBAN">
        <ots:Compatibility GtuType="CAR"/>
      </ots:LaneType>
    </ots:LaneTypes>
    <ots:DetectorTypes>
      <ots:DetectorType Id="sink">
        <ots:Compatibility GtuType="CAR"/>
      </ots:DetectorType>
    </ots:DetectorTypes>
    <ots:RoadLayouts>
      <ots:RoadLayout Id="turnlane" LinkType="URBAN">
        <ots:Shoulder>
          <ots:Width>2m</ots:Width>
        </ots:Shoulder>
        <ots:Lane Id="lane1" LaneType="URBAN">
          <ots:Width>3.5m</ots:Width>
          <ots:SpeedLimit GtuType="CAR" LegalSpeedLimit="40km/h"/>
        </ots:Lane>
        <ots:Shoulder>
          <ots:Width>2m</ots:Width>
        </ots:Shoulder>
      </ots:RoadLayout>
      <ots:RoadLayout Id="2lane" LinkType="URBAN">
        <ots:Shoulder>
          <ots:Width>2m</ots:Width>
        </ots:Shoulder>
        <ots:Stripe Type="SOLID"/>
        <ots:Lane Id="lane2" LaneType="URBAN">
          <ots:Width>3.5m</ots:Width>
        </ots:Lane>
        <ots:Stripe Type="DASHED"/>
        <ots:Lane Id="lane1" LaneType="URBAN">
          <ots:Width>3.5m</ots:Width>
        </ots:Lane>
        <ots:Stripe Type="SOLID"/>
        <ots:Shoulder>
          <ots:Width>2m</ots:Width>
        </ots:Shoulder>
      </ots:RoadLayout>
      <ots:RoadLayout Id="2turnlane" LinkType="URBAN">
        <ots:Shoulder>
          <ots:Width>2m</ots:Width>
        </ots:Shoulder>
        <ots:Lane Id="lane2" LaneType="URBAN">
          <ots:Width>3.5m</ots:Width>
        </ots:Lane>
        <ots:Stripe Type="DASHED"/>
        <ots:Lane Id="lane1" LaneType="URBAN">
          <ots:Width>3.5m</ots:Width>
        </ots:Lane>
        <ots:Shoulder>
          <ots:Width>2m</ots:Width>
        </ots:Shoulder>
      </ots:RoadLayout>
      <ots:RoadLayout Id="4lane" LinkType="URBAN">
        <ots:Shoulder>
          <ots:Width>2m</ots:Width>
        </ots:Shoulder>
        <ots:Stripe Type="SOLID"/>
        <ots:Lane Id="lane4" LaneType="URBAN">
          <ots:Width>3.5m</ots:Width>
        </ots:Lane>
        <ots:Stripe Type="BLOCK"/>
        <ots:Lane Id="lane3" LaneType="URBAN">
          <ots:Width>3.5m</ots:Width>
        </ots:Lane>
        <ots:Stripe Type="DASHED"/>
        <ots:Lane Id="lane2" LaneType="URBAN">
          <ots:Width>3.5m</ots:Width>
        </ots:Lane>
        <ots:Stripe Type="BLOCK"/>
        <ots:Lane Id="lane1" LaneType="URBAN">
          <ots:Width>3.5m</ots:Width>
        </ots:Lane>
        <ots:Stripe Type="SOLID"/>
        <ots:Shoulder>
          <ots:Width>2m</ots:Width>
        </ots:Shoulder>
      </ots:RoadLayout>
    </ots:RoadLayouts>
  </ots:Definitions>
