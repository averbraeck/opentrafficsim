    <ots:Node Id="NB" Coordinate="(-15.9460,8.5808)" Direction="237.0623 deg(E)" />
    <ots:Link Id="NENTERNA" NodeStart="NENTER" NodeEnd="NA" Type="URBAN">
      <ots:Bezier Shape="0.8" Weighted="true" />
      <ots:DefinedLayout>2LANE</ots:DefinedLayout>
