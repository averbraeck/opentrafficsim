    <ots:Node Id="WEXIT" Coordinate="(-29.0000,1.7500)" Direction="180.0000 deg(E)" />
    <ots:Node Id="WA" Coordinate="(-15.9460,-12.0808)" Direction="302.9377 deg(E)" />
