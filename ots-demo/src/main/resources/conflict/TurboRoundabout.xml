          <ots:CenterOffset>-5m</ots:CenterOffset>
          <ots:Width>3m</ots:Width>
        </ots:Shoulder>
        <ots:Stripe Type="SOLID">
          <ots:CenterOffset>-3.5m</ots:CenterOffset>
        </ots:Stripe>
        <ots:Lane Id="LANE" LaneType="URBAN">
