      </ots:LinkType>
    </ots:LinkTypes>

    <ots:LaneTypes>
      <ots:LaneType Id="URBAN">
        <ots:Compatibility GtuType="NL.VEHICLE" />
      </ots:LaneType>
    </ots:LaneTypes>

