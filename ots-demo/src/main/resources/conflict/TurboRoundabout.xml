    <ots:ShortestRoute Id="WS2" GtuType="NL.CAR">
      <ots:From>W</ots:From>
      <ots:To>S2</ots:To>
    </ots:ShortestRoute>
    <ots:ShortestRoute Id="WE2" GtuType="NL.CAR">
      <ots:From>W</ots:From>
      <ots:To>E2</ots:To>
    </ots:ShortestRoute>
    <ots:ShortestRoute Id="WN2" GtuType="NL.CAR">
      <ots:From>W</ots:From>
      <ots:To>N2</ots:To>
    </ots:ShortestRoute>

    <ots:ShortestRoute Id="SE2" GtuType="NL.CAR">
