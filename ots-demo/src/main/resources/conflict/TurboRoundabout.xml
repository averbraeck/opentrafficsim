  <ots:Demand>

    <ots:ShortestRoute Id="EN2" GtuType="NL.CAR">
      <ots:From>E</ots:From>
      <ots:To>N2</ots:To>
    </ots:ShortestRoute>
    <ots:ShortestRoute Id="EW2" GtuType="NL.CAR">
      <ots:From>E</ots:From>
      <ots:To>W2</ots:To>
    </ots:ShortestRoute>
    <ots:ShortestRoute Id="ES2" GtuType="NL.CAR">
      <ots:From>E</ots:From>
      <ots:To>S2</ots:To>
    </ots:ShortestRoute>

    <ots:ShortestRoute Id="NW2" GtuType="NL.CAR">
      <ots:From>N</ots:From>
      <ots:To>W2</ots:To>
    </ots:ShortestRoute>
    <ots:ShortestRoute Id="NS2" GtuType="NL.CAR">
      <ots:From>N</ots:From>
      <ots:To>S2</ots:To>
    </ots:ShortestRoute>
    <ots:ShortestRoute Id="NE2" GtuType="NL.CAR">
      <ots:From>N</ots:From>
      <ots:To>E2</ots:To>
