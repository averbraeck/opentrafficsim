    <ots:RunLength>1h</ots:RunLength>
    <ots:RandomStreams>
      <ots:RandomStream Id="default">
        <ots:Replication Id="1" Seed="1" />
      </ots:RandomStream>
      <ots:RandomStream Id="generation">
        <ots:Replication Id="1" Seed="1" />
      </ots:RandomStream>
    </ots:RandomStreams>
  </ots:Run>

  <ots:Animation>
    <ots:Defaults>
      <ots:Shoulder Color="RGB(0,128,0)" />
    </ots:Defaults>
  </ots:Animation>

</ots:Ots>