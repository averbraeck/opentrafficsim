    <ots:Node Id="N" Coordinate="(0,300)" Direction="-90 deg(E)" />
    <ots:Node Id="W" Coordinate="(-300,0)" Direction="0 deg(E)" />
    <ots:Node Id="S" Coordinate="(0,-300)" Direction="90 deg(E)" />
    <ots:Node Id="E2" Coordinate="(300,0)" Direction="0 deg(E)" />
