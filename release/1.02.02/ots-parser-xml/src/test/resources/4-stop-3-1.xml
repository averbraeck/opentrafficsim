    </LINK>

    <LINK ID="NS45" NODESTART="NS4" NODEEND="NS5" TYPE="STREET">
      <STRAIGHT />
      <DEFINEDLAYOUT>r1</DEFINEDLAYOUT>
    </LINK>

    <LINK ID="NS56" NODESTART="NS5" NODEEND="NS6" TYPE="STREET">
      <STRAIGHT />
