      <FROM NODE="SN1" />
      <TO NODE="EW6" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>
    <SHORTESTROUTE ID="WN" GTUTYPE="VEHICLE">
      <FROM NODE="WE1" />
      <TO NODE="SN6" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>
    <SHORTESTROUTE ID="WE" GTUTYPE="VEHICLE">
