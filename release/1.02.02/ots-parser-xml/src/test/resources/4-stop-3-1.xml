    <LINK ID="EW12" NODESTART="EW1" NODEEND="EW2" TYPE="STREET">
      <STRAIGHT />
      <DEFINEDLAYOUT>r3</DEFINEDLAYOUT>
    </LINK>

    <LINK ID="EW23" NODESTART="EW2" NODEEND="EW3" TYPE="STREET">
      <STRAIGHT />
      <DEFINEDLAYOUT>r3</DEFINEDLAYOUT>
