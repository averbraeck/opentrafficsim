<?xml version="1.0" encoding="UTF-8" ?>
<NETWORK xmlns="http://www.opentrafficsim.org/ots" xmlns:ots="http://www.opentrafficsim.org/ots" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opentrafficsim.org/ots http://www.opentrafficsim.org/docs/current/ots-network.xsd" xmlns:xi="http://www.w3.org/2001/XInclude">

  <DEFINITIONS>

    <GLOBAL />

    <GTUTYPE NAME="CAR" />

    <GTU NAME="CAR" GTUTYPE="CAR" LENGTH="CONST(4.5) m" WIDTH="CONST(2) m"
      MAXSPEED="CONST(80) km/h" />

    <ROADTYPE NAME="HIGHWAY" DEFAULTLANEWIDTH="3m" DEFAULTOVERTAKING="NONE" DEFAULTLANEKEEPING="KEEPLANE">
      <SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="120 km/h"/>
    </ROADTYPE>

    <ROADLAYOUT NAME="RT1ln_ref-4" ROADTYPE="HIGHWAY">
      <LANE NAME="A1" OFFSET="-12m" WIDTH="3m" DIRECTION="FORWARD" />
    </ROADLAYOUT>

    <ROADLAYOUT NAME="RT1ln_ref-3" ROADTYPE="HIGHWAY">
      <LANE NAME="A1" OFFSET="-9m" WIDTH="3m" DIRECTION="FORWARD" />
    </ROADLAYOUT>

    <ROADLAYOUT NAME="RT1ln_ref-2" ROADTYPE="HIGHWAY">
      <LANE NAME="A1" OFFSET="-6m" WIDTH="3m" DIRECTION="FORWARD" />
    </ROADLAYOUT>

    <ROADLAYOUT NAME="RT1ln_ref0" ROADTYPE="HIGHWAY">
      <LANE NAME="A1" OFFSET="0m" WIDTH="3m" DIRECTION="FORWARD" />
    </ROADLAYOUT>

    <ROADLAYOUT NAME="RT3ln_ref-2" ROADTYPE="HIGHWAY">
      <LANE NAME="A1" OFFSET="-12m" WIDTH="3m" DIRECTION="FORWARD" />
      <STRIPE TYPE="DASHED" OFFSET="-10.5m" />
      <LANE NAME="A2" OFFSET="-9m" WIDTH="3m" DIRECTION="FORWARD" />
      <STRIPE TYPE="DASHED" OFFSET="-7.5m" />
      <LANE NAME="A3" OFFSET="-6m" WIDTH="3m" DIRECTION="FORWARD" />
    </ROADLAYOUT>

  </DEFINITIONS>

  <NODE NAME="N_A1" COORDINATE="(-300, 0)" ANGLE="0 deg" />
  <NODE NAME="N_A2" COORDINATE="(-200, 0)" />
  <NODE NAME="N_A3" COORDINATE="(-20, 0)" />

  <NODE NAME="N_B1" COORDINATE="(0, 300)" />
  <NODE NAME="N_B2" COORDINATE="(0, 200)" />
  <NODE NAME="N_B3" COORDINATE="(0, 20)" />

  <NODE NAME="N_C1" COORDINATE="(300, 0)" />
  <NODE NAME="N_C2" COORDINATE="(200, 0)" />
  <NODE NAME="N_C3" COORDINATE="(20, 0)" />

  <NODE NAME="N_D1" COORDINATE="(0, -300)" />
  <NODE NAME="N_D2" COORDINATE="(0, -200)" />
  <NODE NAME="N_D3" COORDINATE="(0, -20)" />

  <LINK NAME="L_A1-A2" NODESTART="N_A1" NODEEND="N_A2" ROADLAYOUT="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_A2-A3" NODESTART="N_A2" NODEEND="N_A3" ROADLAYOUT="RT3ln_ref-2">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_A3-D3" NODESTART="N_A3" NODEEND="N_D3" ROADLAYOUT="RT1ln_ref0" OFFSETSTART="-12m" OFFSETEND="-9m">
    <BEZIER />
  </LINK>
  <LINK NAME="L_A3-C3" NODESTART="N_A3" NODEEND="N_C3" ROADLAYOUT="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_A3-B3" NODESTART="N_A3" NODEEND="N_B3" ROADLAYOUT="RT1ln_ref0" OFFSETSTART="-6m" OFFSETEND="-9m"
    ROTATIONSTART="0 deg" ROTATIONEND="90 deg">
    <BEZIER />
  </LINK>

  <LINK NAME="L_C1-C2" NODESTART="N_C1" NODEEND="N_C2" ROADLAYOUT="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_C2-C3" NODESTART="N_C2" NODEEND="N_C3" ROADLAYOUT="RT3ln_ref-2">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_B3_C3" NODESTART="N_B3" NODEEND="N_C3" ROADLAYOUT="RT1ln_ref0" OFFSETEND="-9m" OFFSETSTART="9m"
    ROTATIONSTART="270 deg" ROTATIONEND="0 deg">
    <BEZIER />
  </LINK>
  <LINK NAME="L_C3-A3" NODESTART="N_C3" NODEEND="N_A3" ROADLAYOUT="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_D3_C3" NODESTART="N_D3" NODEEND="N_C3" ROADLAYOUT="RT1ln_ref0" OFFSETEND="-9m" OFFSETSTART="9m"
    ROTATIONSTART="90 deg" ROTATIONEND="0 deg">
    <BEZIER />
  </LINK>


  <LINK NAME="L_A3-A1" NODESTART="N_A3" NODEEND="N_A1" ROADLAYOUT="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_B3-B1" NODESTART="N_B3" NODEEND="N_B1" ROADLAYOUT="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_C3-C1" NODESTART="N_C3" NODEEND="N_C1" ROADLAYOUT="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_D3-D1" NODESTART="N_D3" NODEEND="N_D1" ROADLAYOUT="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>

</NETWORK>