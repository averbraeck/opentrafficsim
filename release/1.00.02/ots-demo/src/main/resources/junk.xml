<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE xml>
<NETWORK xmlns="http://www.opentrafficsim.org/ots" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.opentrafficsim.org/ots http://www.opentrafficsim.org/docs/current/ots-network.xsd">

	<DEFINITIONS>

	<GLOBAL />

		<GTUTYPE NAME="CAR" />
		<GTUTYPE NAME="TRUCK" />

		<GTU NAME="CAR" GTUTYPE="CAR" LENGTH="UNIF(4,7) m" WIDTH="UNIF(1.7, 2) m" MAXSPEED="CONST(120) km/h" />
		<GTU NAME="TRUCK" GTUTYPE="TRUCK" LENGTH="UNIF(16,24) m" WIDTH="UNIF(2.2, 2.7) m" MAXSPEED="CONST(100) km/h" />

		<GTUMIX NAME="gtumix">
			<GTU NAME="CAR" WEIGHT="0.0"></GTU>
			<GTU NAME="TRUCK" WEIGHT="100.0"></GTU>
		</GTUMIX>

		<ROADTYPE NAME="NORMALROAD" DEFAULTLANEWIDTH="3.5m" DEFAULTLANEKEEPING="KEEPRIGHT"
			DEFAULTOVERTAKING="LEFTSET([CAR, TRUCK] OVERTAKE [ALL]) RIGHTSPEED(40 km/h)">
			<SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="60km/h" />
		</ROADTYPE>
		<ROADLAYOUT NAME="BRANCH" ROADTYPE="NORMALROAD">
			<SHOULDER WIDTH="2m" OFFSET="-1m" COLOR="GREEN" />
			<STRIPE TYPE="SOLID" OFFSET="1m" WIDTH="20cm" />
			<LANE NAME="Lane1" OFFSET="1.75m" DIRECTION="FORWARD" />
			<STRIPE TYPE="SOLID" OFFSET="3.5m" WIDTH="20cm" />
			<SHOULDER WIDTH="2m" OFFSET="4.5m" COLOR="GREEN" />
		</ROADLAYOUT>
		<ROADLAYOUT NAME="MAIN" ROADTYPE="NORMALROAD">
			<SHOULDER WIDTH="2m" OFFSET="-1m" COLOR="GREEN" />
			<STRIPE TYPE="SOLID" OFFSET="1m" WIDTH="20cm" />
			<LANE NAME="Lane1" OFFSET="1.75m" DIRECTION="FORWARD" />
			<STRIPE TYPE="SOLID" OFFSET="3.5m" WIDTH="20cm" />
			<SHOULDER WIDTH="2m" OFFSET="4.5m" COLOR="GREEN" />
		</ROADLAYOUT>
		<ROADLAYOUT NAME="COMMON" ROADTYPE="NORMALROAD">
			<SHOULDER WIDTH="2m" OFFSET="-1m" COLOR="GREEN" />
			<STRIPE TYPE="SOLID" OFFSET="1m" WIDTH="20cm" />
			<LANE NAME="Lane1" OFFSET="1.75m" DIRECTION="FORWARD" />
			<STRIPE TYPE="DASHED" OFFSET="1m" WIDTH="20cm" />
			<LANE NAME="Lane2" OFFSET="5.25m" DIRECTION="FORWARD" />
			<STRIPE TYPE="SOLID" OFFSET="7.0m" WIDTH="20cm" />
			<SHOULDER WIDTH="2m" OFFSET="8.0m" COLOR="GREEN" />
		</ROADLAYOUT>
		<ROADLAYOUT NAME="COMMONCOMPRESSED" ROADTYPE="NORMALROAD">
			<SHOULDER WIDTH="2m" OFFSET="-1m" COLOR="GREEN" />
			<STRIPE TYPE="SOLID" OFFSET="1m" WIDTH="20cm" />
			<LANE NAME="Lane1" OFFSET="1.75m" DIRECTION="FORWARD" />
			<STRIPE TYPE="SOLID" OFFSET="3.5m" WIDTH="20cm" />
			<SHOULDER WIDTH="2m" OFFSET="4.5m" COLOR="GREEN" />
		</ROADLAYOUT>
	</DEFINITIONS>
	<NODE NAME="From" COORDINATE="(0,0,0)" ANGLE="0 deg" />
	<NODE NAME="End" COORDINATE="(2000,0,0)" ANGLE="0 deg" />
	<NODE NAME="From2" COORDINATE="(0,-50,0)" ANGLE="0 deg" />
	<NODE NAME="FirstVia" COORDINATE="(500,0,0)" ANGLE="0 deg" />
	<NODE NAME="SecondVia" COORDINATE="(1000,0,0)" ANGLE="0 deg" />
	<NODE NAME="End2" COORDINATE="(2000,-50,0)" ANGLE="0 deg" />
	<ROUTE NAME="ALL" NODELIST="FirstVia SecondVia End" />	<ROUTEMIX NAME="routemix" >
		<ROUTE WEIGHT="1" NAME="ALL" />
	</ROUTEMIX>
	<LINK NAME="From2 to FirstVia" NODESTART="From2" NODEEND="FirstVia" ROADLAYOUT="BRANCH">
		<STRAIGHT />
		<GENERATOR LANE="Lane1" POSITION="16m" IAT="ERLANG(4,4.2) s" INITIALSPEED="UNIFORM(80,90) km/h" GTUMIX="gtumix" ROUTEMIX="routemix"
			GTUCOLORER="SWITCHABLE" />
	</LINK>
	<LINK NAME="From to FirstVia" NODESTART="From2" NODEEND="FirstVia" ROADLAYOUT="MAIN">
		<STRAIGHT />
		<GENERATOR LANE="Lane1" POSITION="16m" IAT="ERLANG(4,4.2) s" INITIALSPEED="UNIFORM(80,90) km/h" GTUMIX="gtumix" ROUTEMIX="routemix"
			GTUCOLORER="SWITCHABLE" />
	</LINK>
	<LINK NAME="FirstVia to SecondVia" NODESTART="FirstVia" NODEEND="SecondVia" ROADLAYOUT="COMMON">
		<STRAIGHT />
		<BLOCK LANE="Lane2" POSITION="END-1mm" />
	</LINK>
	<LINK NAME="SecondVia to End2" NODESTART="SecondVia" NODEEND="End2" ROADLAYOUT="BRANCH">
		<STRAIGHT />
		<SINK POSITION="999m" LANE="Lane1" />
	</LINK>
	<LINK NAME="SecondVia to End" NODESTART="SecondVia" NODEEND="End" ROADLAYOUT="BRANCH">		<STRAIGHT />
		<SINK POSITION="999m" LANE="Lane1" />
	</LINK>
</NETWORK>
