<?xml version="1.0" encoding="UTF-8"?>
<OTS xmlns="http://www.opentrafficsim.org/ots" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opentrafficsim.org/ots ../../../../ots-xsd/src/main/resources/xsd/1.01.02/ots.xsd">

  <DEFINITIONS>
    <LINKTYPES>
      <LINKTYPE NAME="HIGHWAY">
      </LINKTYPE>
    </LINKTYPES>
    <GTUTEMPLATES />
    <GTUTYPES />
    <LANETYPES>
      <LANETYPE NAME="HIGHWAYLANE"></LANETYPE>
    </LANETYPES>
    <ROADLAYOUTS>
      <ROADLAYOUT NAME="Broad" LINKTYPE="HIGHWAY">
        <LANE NAME="LEFT" LANETYPE="HIGHWAYLANE" DIRECTION="FORWARD">
          <CENTEROFFSET>-3.5m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="120km/h" />
      </ROADLAYOUT>
    </ROADLAYOUTS>
  </DEFINITIONS>

  <NETWORK>
    <NODE COORDINATE="(0,0)" NAME="A" />
    <NODE COORDINATE="(1,0)" NAME="B" />
    <LINK NODESTART="A" NODEEND="B" NAME="AB">
      <STRAIGHT />
      <TRAFFICLIGHT NAME="AB" CONTROLLER="N333-dorpsweg" LANE="LEFT" POSITION="300m" CLASS="simpleTrafficLight" />
    </LINK>

    <SIGNALGROUP ID="asd">
      <TRAFFICLIGHT NAME="ABLane1" LINK="AB" LANE="Lane1" POSITION="100m" CLASS="SimpleTrafficLight" CONTROLLER="N333-dorpsweg" />
    </SIGNALGROUP>
  </NETWORK>

  <NETWORKDEMAND>
    <OD></OD>
  </NETWORKDEMAND>

  <CONTROL>
    <FIXEDTIME ID="N333-dorpsweg" CYCLETIME="90s" OFFSET="12s" STARTTIME="0s" ENDTIME="1000000h">
      <SIGNALGROUP ID="asd" OFFSET="0s" GREEN="20s" YELLOW="3.5s" />
    </FIXEDTIME>
  </CONTROL>

  <MODEL></MODEL>
  <SCENARIO></SCENARIO>
  <RUN RUNLENGTH="1h"></RUN>
  <ANIMATION>
    <GTUCOLORER NAME="DEFAULT"></GTUCOLORER>
  </ANIMATION>

</OTS>