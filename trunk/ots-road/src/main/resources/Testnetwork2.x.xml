<?xml version="1.0" encoding="UTF-8" ?>
<NETWORK xmlns="http://www.opentrafficsim.org/ots" xmlns:ots="http://www.opentrafficsim.org/ots" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opentrafficsim.org/ots http://www.opentrafficsim.org/docs/ots-network.xsd" xmlns:xi="http://www.w3.org/2001/XInclude">

  <DEFINITIONS>

    <GLOBAL DEFAULTLANEWIDTH="3m" DEFAULTMAXSPEED="100 km/h" DEFAULTOVERTAKING="NONE" DEFAULTLANEKEEPING="KEEPLANE"></GLOBAL>

    <GTUTYPE NAME="CAR" />

    <GTU NAME="CAR" GTUTYPE="CAR" FOLLOWING="IDM+" LENGTH="CONST(4.5) m" WIDTH="CONST(2) m" LANECHANGE="EGOISTIC"
      MAXSPEED="CONST(80) km/h" />

    <LANETYPE NAME="HIGHWAY" />

    <COMPATIBILITY LANETYPE="HIGHWAY">
      <GTU NAME="CAR" />
    </COMPATIBILITY>

    <ROADTYPE NAME="RT1ln_ref-4">
      <LANE NAME="A1" TYPE="HIGHWAY" OFFSET="-12m" WIDTH="3m" DIRECTION="FORWARD" SPEED="120km/h" />
    </ROADTYPE>

    <ROADTYPE NAME="RT1ln_ref-3">
      <LANE NAME="A1" TYPE="HIGHWAY" OFFSET="-9m" WIDTH="3m" DIRECTION="FORWARD" SPEED="120km/h" />
    </ROADTYPE>

    <ROADTYPE NAME="RT1ln_ref-2">
      <LANE NAME="A1" TYPE="HIGHWAY" OFFSET="-6m" WIDTH="3m" DIRECTION="FORWARD" SPEED="120km/h" />
    </ROADTYPE>

    <ROADTYPE NAME="RT1ln_ref0">
      <LANE NAME="A1" TYPE="HIGHWAY" OFFSET="0m" WIDTH="3m" DIRECTION="FORWARD" SPEED="120km/h" />
    </ROADTYPE>

    <ROADTYPE NAME="RT3ln_ref-2">
      <LANE NAME="A1" TYPE="HIGHWAY" OFFSET="-12m" WIDTH="3m" DIRECTION="FORWARD" SPEED="120km/h" />
      <STRIPE TYPE="DASHED" OFFSET="-10.5m" />
      <LANE NAME="A2" TYPE="HIGHWAY" OFFSET="-9m" WIDTH="3m" DIRECTION="FORWARD" SPEED="120km/h" />
      <STRIPE TYPE="DASHED" OFFSET="-7.5m" />
      <LANE NAME="A3" TYPE="HIGHWAY" OFFSET="-6m" WIDTH="3m" DIRECTION="FORWARD" SPEED="120km/h" />
    </ROADTYPE>

  </DEFINITIONS>

  <NODE NAME="N_A1" COORDINATE="(-300, 0)" ANGLE="0 deg" />
  <NODE NAME="N_A2" COORDINATE="(-200, 0)" />
  <NODE NAME="N_A3" COORDINATE="(-20, 0)" />

  <NODE NAME="N_B1" COORDINATE="(0, 300)" />
  <NODE NAME="N_B2" COORDINATE="(0, 200)" />
  <NODE NAME="N_B3" COORDINATE="(0, 20)" />

  <NODE NAME="N_C1" COORDINATE="(300, 0)" />
  <NODE NAME="N_C2" COORDINATE="(200, 0)" />
  <NODE NAME="N_C3" COORDINATE="(20, 0)" />

  <NODE NAME="N_D1" COORDINATE="(0, -300)" />
  <NODE NAME="N_D2" COORDINATE="(0, -200)" />
  <NODE NAME="N_D3" COORDINATE="(0, -20)" />

  <LINK NAME="L_A1-A2" NODESTART="N_A1" NODEEND="N_A2" ROADTYPE="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_A2-A3" NODESTART="N_A2" NODEEND="N_A3" ROADTYPE="RT3ln_ref-2">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_A3-D3" NODESTART="N_A3" NODEEND="N_D3" ROADTYPE="RT1ln_ref0" OFFSETSTART="-12m" OFFSETEND="-9m">
    <BEZIER />
  </LINK>
  <LINK NAME="L_A3-C3" NODESTART="N_A3" NODEEND="N_C3" ROADTYPE="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_A3-B3" NODESTART="N_A3" NODEEND="N_B3" ROADTYPE="RT1ln_ref0" OFFSETSTART="-6m" OFFSETEND="-9m"
    ROTATIONSTART="0 deg" ROTATIONEND="90 deg">
    <BEZIER />
  </LINK>

  <LINK NAME="L_C1-C2" NODESTART="N_C1" NODEEND="N_C2" ROADTYPE="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_C2-C3" NODESTART="N_C2" NODEEND="N_C3" ROADTYPE="RT3ln_ref-2">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_B3_C3" NODESTART="N_B3" NODEEND="N_C3" ROADTYPE="RT1ln_ref0" OFFSETEND="-9m" OFFSETSTART="9m"
    ROTATIONSTART="270 deg" ROTATIONEND="0 deg">
    <BEZIER />
  </LINK>
  <LINK NAME="L_C3-A3" NODESTART="N_C3" NODEEND="N_A3" ROADTYPE="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_D3_C3" NODESTART="N_D3" NODEEND="N_C3" ROADTYPE="RT1ln_ref0" OFFSETEND="-9m" OFFSETSTART="9m"
    ROTATIONSTART="90 deg" ROTATIONEND="0 deg">
    <BEZIER />
  </LINK>


  <LINK NAME="L_A3-A1" NODESTART="N_A3" NODEEND="N_A1" ROADTYPE="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_B3-B1" NODESTART="N_B3" NODEEND="N_B1" ROADTYPE="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_C3-C1" NODESTART="N_C3" NODEEND="N_C1" ROADTYPE="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>
  <LINK NAME="L_D3-D1" NODESTART="N_D3" NODEEND="N_D1" ROADTYPE="RT1ln_ref-3">
    <STRAIGHT />
  </LINK>

</NETWORK>