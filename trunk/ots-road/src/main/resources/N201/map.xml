<?xml version="1.0" encoding="UTF-8" ?>
<!-- Created by Alexander Verbraeck on Apr 30, 2014 -->
<gisbeans:map xmlns:gisbeans="http://www.javel.nl/gisbeans">

	<!-- We start with the global map attributes -->
	<name>N201</name>
	<units>dd</units>
	<!-- extent>
		<minX>-122.071</minX>
		<minY>37.402</minY>
		<maxX>-121.992</maxX>
		<maxY>37.435</maxY>
	</extent -->
  <extent>
    <minX>0</minX>
    <minY>0</minY>
    <maxX>150000</maxX>
    <maxY>500000</maxY>
  </extent>
	<gisbeans:image>
		<backgroundColor>
			<r>255</r>
			<g>255</g>
			<b>255</b>
		</backgroundColor>
		<size>
			<height>2000</height>
			<width>2000</width>
		</size>
	</gisbeans:image>

	<!-- Now we get the layers -->
	<gisbeans:layer>
		<name>roads</name>
		<data>
			<shapeFile>/N201/roads.shp</shapeFile>
		</data>
		<color>
			<r>120</r>
			<g>120</g>
			<b>120</b>
		</color>
		<outlineColor>
			<r>120</r>
			<g>120</g>
			<b>120</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	<gisbeans:layer>
		<name>buildings</name>
		<data>
			<shapeFile>/N201/buildings.shp</shapeFile>
		</data>
		<color>
			<r>200</r>
			<g>200</g>
			<b>200</b>
		</color>
		<outlineColor>
			<r>200</r>
			<g>200</g>
			<b>200</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	<gisbeans:layer>
		<name>natural</name>
		<data>
			<shapeFile>/N201/natural.shp</shapeFile>
		</data>
		<color>
			<r>127</r>
			<g>255</g>
			<b>197</b>
		</color>
		<outlineColor>
			<r>0</r>
			<g>255</g>
			<b>0</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	<gisbeans:layer>
		<name>railways</name>
		<data>
			<shapeFile>/N201/railways.shp</shapeFile>
		</data>
		<color>
			<r>0</r>
			<g>19</g>
			<b>127</b>
		</color>
		<outlineColor>
			<r>0</r>
			<g>19</g>
			<b>127</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	<gisbeans:layer>
		<name>waterways</name>
		<data>
			<shapeFile>/N201/waterways.shp</shapeFile>
		</data>
		<color>
			<r>0</r>
			<g>148</g>
			<b>255</b>
		</color>
		<outlineColor>
			<r>0</r>
			<g>148</g>
			<b>255</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
</gisbeans:map>
