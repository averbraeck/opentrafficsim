    </GTUTYPES>
    <GTUTEMPLATES>
      <GTUTEMPLATE GTUTYPE="CAR" ID="CARS">
        <LENGTHDIST LENGTHUNIT="m">
          <UNIFORM MIN="4" MAX="7" />
        </LENGTHDIST>
        <WIDTHDIST LENGTHUNIT="m">
          <UNIFORM MIN="1.7" MAX="2" />
        </WIDTHDIST>
        <MAXSPEEDDIST SPEEDUNIT="km/h">
          <CONSTANT C="120" />
        </MAXSPEEDDIST>
      </GTUTEMPLATE>
      <GTUTEMPLATE GTUTYPE="TRUCK" ID="TRUCKS">
