<?xml version="1.0" encoding="UTF-8" ?>
<OTS xmlns="http://www.opentrafficsim.org/ots" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opentrafficsim.org/ots https://opentrafficsim.org/docs/xsd/1.02.01/ots.xsd"
  xmlns:xi="http://www.w3.org/2001/XInclude">

  <DEFINITIONS>
    <xi:include href="https://opentrafficsim.org/docs/xsd/1.02.01/defaults/default_gtutypes.xml" />
    <xi:include href="https://opentrafficsim.org/docs/xsd/1.02.01/defaults/default_linktypes.xml" />
    <xi:include href="https://opentrafficsim.org/docs/xsd/1.02.01/defaults/default_lanetypes.xml" />

    <GTUTEMPLATES>
      <GTUTEMPLATE DEFAULT="true" GTUTYPE="CAR" ID="CAR">
        <LENGTHDIST LENGTHUNIT="m">
          <UNIFORM MIN="3.0" MAX="4.0" />
        </LENGTHDIST>
        <WIDTHDIST LENGTHUNIT="m">
          <UNIFORM MIN="1.7" MAX="2.0" />
        </WIDTHDIST>
        <MAXSPEEDDIST SPEEDUNIT="km/h">
          <CONSTANT C="120" />
        </MAXSPEEDDIST>
      </GTUTEMPLATE>
      <GTUTEMPLATE DEFAULT="true" GTUTYPE="TRUCK" ID="TRUCK">
        <LENGTHDIST LENGTHUNIT="m">
          <UNIFORM MIN="16.0" MAX="24.0" />
        </LENGTHDIST>
        <WIDTHDIST LENGTHUNIT="m">
          <UNIFORM MIN="2.2" MAX="2.7" />
        </WIDTHDIST>
        <MAXSPEEDDIST SPEEDUNIT="km/h">
          <CONSTANT C="100" />
        </MAXSPEEDDIST>
      </GTUTEMPLATE>
    </GTUTEMPLATES>

    <LINKTYPES>
      <LINKTYPE ID="HIGHWAY">
        <COMPATIBILITY GTUTYPE="VEHICLE" DIRECTION="FORWARD" />
        <SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="100km/h" />
        <SPEEDLIMIT GTUTYPE="TRUCK" LEGALSPEEDLIMIT="90km/h" />
      </LINKTYPE>
      <LINKTYPE ID="STREET">
        <COMPATIBILITY GTUTYPE="VEHICLE" DIRECTION="FORWARD" />
        <SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="50km/h" />
        <SPEEDLIMIT GTUTYPE="TRUCK" LEGALSPEEDLIMIT="50km/h" />
      </LINKTYPE>
    </LINKTYPES>


    <GTUMIX NAME="C60T40">
      <GTU NAME="CAR" WEIGHT="60"></GTU>
      <GTU NAME="TRUCK" WEIGHT="40"></GTU>
    </GTUMIX>

    <GTUMIX NAME="C80T20">
      <GTU NAME="CAR" WEIGHT="80" />
      <GTU NAME="TRUCK" WEIGHT="20" />
    </GTUMIX>

    <LANETYPE NAME="NOTRUCK">
      <SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="100km/h" />
    </LANETYPE>

    <ROADLAYOUTS>
      <ROADLAYOUT ID="r1" LINKTYPE="STREET">
        <SHOULDER><CENTEROFFSET>-2.75m</CENTEROFFSET><WIDTH>2m</WIDTH></SHOULDER>WIDTH="2m" OFFSET="-12.00m" COLOR="GREEN" />
        <STRIPE TYPE="SOLID"><CENTEROFFSET>-11.0m</CENTEROFFSET></STRIPE>
        <LANE ID="FORWARD3" OFFSET="-9.4m" WIDTH="3.3m" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>0.0m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="DASHED" OFFSET="-7.8m" WIDTH="20cm" COLOR="#FFFFFF" />
        <LANE ID="FORWARD2" OFFSET="-6.2m" WIDTH="3.3m" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>0.0m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="DASHED" OFFSET="-4.6m" WIDTH="20cm" COLOR="#FFFFFF" />
        <LANE ID="FORWARD1" OFFSET="-3m" WIDTH="3.3m" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>0.0m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID" OFFSET="-1.4m" WIDTH="20cm" COLOR="#FFFFFF" />
        <NOTRAFFICLANE OFFSET="0m" WIDTH="2.5m" COLOR="GREEN" />
      </ROADLAYOUT>

      <ROADLAYOUT ID="r2r" LINKTYPE="STREET">
        <SHOULDER><CENTEROFFSET>-2.75m</CENTEROFFSET><WIDTH>2m</WIDTH></SHOULDER>WIDTH="2m" OFFSET="-8.8m" COLOR="GREEN" />
        <STRIPE TYPE="SOLID" OFFSET="-7.8m" />
        <LANE ID="FORWARD" OFFSET="-6.2m" WIDTH="3.3m" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>0.0m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID" OFFSET="-4.6m" WIDTH="20cm" COLOR="#FFFFFF" />
      </ROADLAYOUT>

      <ROADLAYOUT ID="r2l" LINKTYPE="STREET">
        <STRIPE TYPE="SOLID" OFFSET="-7.8m" WIDTH="20cm" COLOR="#FFFFFF" />
        <LANE ID="FORWARD" OFFSET="-6.2m" WIDTH="3.3m" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>0.0m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID" OFFSET="-4.6m" WIDTH="20cm" COLOR="#FFFFFF" />
        <NOTRAFFICLANE OFFSET="0m" WIDTH="2.5m" COLOR="GREEN" />
      </ROADLAYOUT>

      <ROADLAYOUT ID="r3" LINKTYPE="STREET">
        <NOTRAFFICLANE OFFSET="-2.3m" WIDTH="4.6m" COLOR="GREEN" />
        <STRIPE TYPE="SOLID" OFFSET="-4.6m" WIDTH="20cm" COLOR="#FFFFFF" />
        <LANE ID="FORWARD" OFFSET="-6.2m" WIDTH="3.3m" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>0.0m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID" OFFSET="-7.8m" WIDTH="20cm" COLOR="#FFFFFF" />
        <NOTRAFFICLANE OFFSET="-8.8m" WIDTH="2m" COLOR="GREEN" />
      </ROADLAYOUT>

      <ROADLAYOUT ID="r4" LINKTYPE="STREET">
        <NOTRAFFICLANE OFFSET="-2.3m" WIDTH="4.6m" COLOR="GREEN" />
        <STRIPE TYPE="SOLID" OFFSET="-4.6m" WIDTH="20cm" COLOR="#FFFFFF" />
        <LANE ID="FORWARD" OFFSET="-6.2m" WIDTH="3.3m" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>0.0m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID" OFFSET="-7.8m" WIDTH="20cm" COLOR="#FFFFFF" />
        <SHOULDER><CENTEROFFSET>-2.75m</CENTEROFFSET><WIDTH>2m</WIDTH></SHOULDER>OFFSET="-8.8m" WIDTH="2m" COLOR="GREEN" />
      </ROADLAYOUT>
    </ROADLAYOUTS>
  </DEFINITIONS>

  <NETWORK>
    <NODE ID="N" COORDINATE="(0,300)" DIRECTION="270 deg" />
    <NODE ID="E" COORDINATE="(300,0)" DIRECTION="180 deg" />
    <NODE ID="S" COORDINATE="(0,-300)" DIRECTION="90 deg" />
    <NODE ID="W" COORDINATE="(-300,0)" DIRECTION="0 deg" />
    <NODE ID="NC" COORDINATE="(0,22)" DIRECTION="270 deg" />
    <NODE ID="EC" COORDINATE="(22,0)" DIRECTION="180 deg" />
    <NODE ID="SC" COORDINATE="(0,-22)" DIRECTION="90 deg" />
    <NODE ID="WC" COORDINATE="(-22,0)" DIRECTION="0 deg" />
    <NODE ID="NCO" COORDINATE="(0,22)" DIRECTION="90 deg" />
    <NODE ID="ECO" COORDINATE="(22,0)" DIRECTION="0 deg" />
    <NODE ID="SCO" COORDINATE="(0,-22)" DIRECTION="270 deg" />
    <NODE ID="WCO" COORDINATE="(-22,0)" DIRECTION="180 deg" />

    <ROUTE NAME="NE" NODELIST="N NC ECO E" />
    <ROUTE NAME="NS" NODELIST="N NC SCO S" />
    <ROUTE NAME="NW" NODELIST="N NC WCO W" />
    <ROUTE NAME="EN" NODELIST="E EC NCO N" />
    <ROUTE NAME="ES" NODELIST="E EC SCO S" />
    <ROUTE NAME="EW" NODELIST="E EC WCO W" />
    <ROUTE NAME="SN" NODELIST="S SC NCO N" />
    <ROUTE NAME="SE" NODELIST="S SC ECO E" />
    <ROUTE NAME="SW" NODELIST="S SC WCO W" />
    <ROUTE NAME="WN" NODELIST="W WC NCO N" />
    <ROUTE NAME="WE" NODELIST="W WC ECO E" />
    <ROUTE NAME="WS" NODELIST="W WC SCO S" />

    <LINK ID="NNC" NODESTART="N" NODEEND="NC" ROADLAYOUT="r1">
      <STRAIGHT />
      <GENERATOR LANE="FORWARD1" POSITION="10m" IAT="EXPO(20) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="NE" GTUCOLORER="SWITCHABLE" />
      <GENERATOR LANE="FORWARD2" POSITION="10m" IAT="EXPO(10) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="NS" GTUCOLORER="SWITCHABLE" />
      <GENERATOR LANE="FORWARD3" POSITION="10m" IAT="EXPO(10) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="NW" GTUCOLORER="SWITCHABLE" />
    </LINK>

    <LINK ID="EEC" NODESTART="E" NODEEND="EC" ROADLAYOUT="r1">
      <STRAIGHT />
      <GENERATOR LANE="FORWARD1" POSITION="10m" IAT="EXPO(20) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="ES" GTUCOLORER="SWITCHABLE" />
      <GENERATOR LANE="FORWARD2" POSITION="10m" IAT="EXPO(10) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="EW" GTUCOLORER="SWITCHABLE" />
      <GENERATOR LANE="FORWARD3" POSITION="10m" IAT="EXPO(10) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="EN" GTUCOLORER="SWITCHABLE" />
    </LINK>

    <LINK ID="SSC" NODESTART="S" NODEEND="SC" ROADLAYOUT="r1">
      <STRAIGHT />
      <GENERATOR LANE="FORWARD1" POSITION="10m" IAT="EXPO(20) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="SW" GTUCOLORER="SWITCHABLE" />
      <GENERATOR LANE="FORWARD2" POSITION="10m" IAT="EXPO(10) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="SN" GTUCOLORER="SWITCHABLE" />
      <GENERATOR LANE="FORWARD3" POSITION="10m" IAT="EXPO(10) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="SE" GTUCOLORER="SWITCHABLE" />
    </LINK>

    <LINK ID="WWC" NODESTART="W" NODEEND="WC" ROADLAYOUT="r1">
      <STRAIGHT />
      <GENERATOR LANE="FORWARD1" POSITION="10m" IAT="EXPO(20) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="WN" GTUCOLORER="SWITCHABLE" />
      <GENERATOR LANE="FORWARD2" POSITION="10m" IAT="EXPO(10) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="WE" GTUCOLORER="SWITCHABLE" />
      <GENERATOR LANE="FORWARD3" POSITION="10m" IAT="EXPO(10) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR"
        ROUTE="WS" GTUCOLORER="SWITCHABLE" />
    </LINK>

    <LINK ID="ECNC" NODESTART="EC" NODEEND="NCO" ROADLAYOUT="r2r" OFFSETSTART="-3.2m" OFFSETEND="0m">
      <BEZIER STARTDIRECTION="180 deg" ENDDIRECTION="90 deg" />
    </LINK>

    <LINK ID="ECSC" NODESTART="EC" NODEEND="SCO" ROADLAYOUT="r2l" OFFSETSTART="3.2m" OFFSETEND="0m">
      <BEZIER STARTDIRECTION="180 deg" ENDDIRECTION="270 deg" />
    </LINK>

    <LINK ID="NCWC" NODESTART="NC" NODEEND="WCO" ROADLAYOUT="r2r" OFFSETSTART="-3.2m" OFFSETEND="0m">
      <BEZIER STARTDIRECTION="270 deg" ENDDIRECTION="180 deg" />
    </LINK>

    <LINK ID="NCEC" NODESTART="NC" NODEEND="ECO" ROADLAYOUT="r2l" OFFSETSTART="3.2m" OFFSETEND="0m">
      <BEZIER STARTDIRECTION="270 deg" ENDDIRECTION="0 deg" />
    </LINK>

    <LINK ID="WCSC" NODESTART="WC" NODEEND="SCO" ROADLAYOUT="r2r" OFFSETSTART="-3.2m" OFFSETEND="0m">
      <BEZIER STARTDIRECTION="0 deg" ENDDIRECTION="270 deg" />
    </LINK>

    <LINK ID="WCNC" NODESTART="WC" NODEEND="NCO" ROADLAYOUT="r2l" OFFSETSTART="3.2m" OFFSETEND="0m">
      <BEZIER STARTDIRECTION="0 deg" ENDDIRECTION="90 deg" />
    </LINK>

    <LINK ID="SCEC" NODESTART="SC" NODEEND="ECO" ROADLAYOUT="r2r" OFFSETSTART="-3.2m" OFFSETEND="0m">
      <BEZIER STARTDIRECTION="90 deg" ENDDIRECTION="0 deg" />
    </LINK>

    <LINK ID="SCWC" NODESTART="SC" NODEEND="WCO" ROADLAYOUT="r2l" OFFSETSTART="3.2m" OFFSETEND="0m">
      <BEZIER STARTDIRECTION="90 deg" ENDDIRECTION="180 deg" />
    </LINK>

    <LINK ID="NCSC" NODESTART="NC" NODEEND="SCO" ROADLAYOUT="r3">
      <STRAIGHT />
    </LINK>

    <LINK ID="WCEC" NODESTART="WC" NODEEND="ECO" ROADLAYOUT="r3">
      <STRAIGHT />
    </LINK>

    <LINK ID="SCNC" NODESTART="SC" NODEEND="NCO" ROADLAYOUT="r3">
      <STRAIGHT />
    </LINK>

    <LINK ID="ECWC" NODESTART="EC" NODEEND="WCO" ROADLAYOUT="r3">
      <STRAIGHT />
    </LINK>

    <LINK ID="ECOE" NODESTART="ECO" NODEEND="E" ROADLAYOUT="r4">
      <STRAIGHT />
      <SINK POSITION="250m" LANE="FORWARD" />
    </LINK>

    <LINK ID="NCON" NODESTART="NCO" NODEEND="N" ROADLAYOUT="r4">
      <STRAIGHT />
      <SINK POSITION="250m" LANE="FORWARD" />
    </LINK>

    <LINK ID="WCOW" NODESTART="WCO" NODEEND="W" ROADLAYOUT="r4">
      <STRAIGHT />
      <SINK POSITION="250m" LANE="FORWARD" />
    </LINK>

    <LINK ID="SCOS" NODESTART="SCO" NODEEND="S" ROADLAYOUT="r4">
      <STRAIGHT />
      <SINK POSITION="250m" LANE="FORWARD" />
    </LINK>

  </NETWORK>

  <NETWORKDEMAND>
  </NETWORKDEMAND>

  <SCENARIO></SCENARIO>

  <RUN>
    <RUNLENGTH>1h</RUNLENGTH>
  </RUN>
</OTS>