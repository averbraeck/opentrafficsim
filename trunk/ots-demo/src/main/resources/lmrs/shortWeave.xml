<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE xml>
<NETWORK xmlns="http://www.opentrafficsim.org/ots" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opentrafficsim.org/ots https://www.opentrafficsim.org/xsd/ots-network-1-00-00.xsd">

  <DEFINITIONS>

    <GLOBAL />

    <ROADTYPE NAME="HIGHWAY" DEFAULTLANEWIDTH="3.6m" DEFAULTLANEKEEPING="KEEPRIGHT"
      DEFAULTOVERTAKING="LEFTSET([CAR, TRUCK] OVERTAKE [ALL]) RIGHTSPEED(40 km/h)">
      <SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="120km/h" />
      <SPEEDLIMIT GTUTYPE="TRUCK" LEGALSPEEDLIMIT="90km/h" />
    </ROADTYPE>
    
    <ROADTYPE NAME="LOCAL" DEFAULTLANEWIDTH="3.6m" DEFAULTLANEKEEPING="KEEPRIGHT"
      DEFAULTOVERTAKING="LEFTSET([CAR, TRUCK] OVERTAKE [ALL]) RIGHTSPEED(40 km/h)">
      <SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="20km/h" />
      <SPEEDLIMIT GTUTYPE="TRUCK" LEGALSPEEDLIMIT="20km/h" />
    </ROADTYPE>

    <ROADLAYOUT NAME="4LaneHighway" ROADTYPE="HIGHWAY">
      <NOTRAFFICLANE OFFSET="12.1m" WIDTH="3m" COLOR="RGB(0,128,0)" />
      <STRIPE TYPE="SOLID" OFFSET="10.8m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD1" OFFSET="9.0m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="DASHED" OFFSET="7.2m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD2" OFFSET="5.4m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="DASHED" OFFSET="3.6m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD3" OFFSET="1.8m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="DASHED" OFFSET="0m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD4" OFFSET="-1.8m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="SOLID" OFFSET="-3.6m" WIDTH="20cm" COLOR="#FFFFFF" />
      <NOTRAFFICLANE OFFSET="-5.1m" WIDTH="3m" COLOR="RGB(0,128,0)" />
    </ROADLAYOUT>

    <ROADLAYOUT NAME="3LaneHighway" ROADTYPE="HIGHWAY">
      <NOTRAFFICLANE OFFSET="12.1m" WIDTH="3m" COLOR="RGB(0,128,0)" />
      <STRIPE TYPE="SOLID" OFFSET="10.8m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD1" OFFSET="9.0m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="DASHED" OFFSET="7.2m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD2" OFFSET="5.4m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="DASHED" OFFSET="3.6m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD3" OFFSET="1.8m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="SOLID" OFFSET="0m" WIDTH="20cm" COLOR="#FFFFFF" />
      <NOTRAFFICLANE OFFSET="-1.5m" WIDTH="3m" COLOR="RGB(0,128,0)" />
    </ROADLAYOUT>

    <ROADLAYOUT NAME="2LaneHighway" ROADTYPE="HIGHWAY">
      <NOTRAFFICLANE OFFSET="8.5m" WIDTH="3m" COLOR="RGB(0,128,0)" />
      <STRIPE TYPE="SOLID" OFFSET="7.2m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD1" OFFSET="5.4m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="DASHED" OFFSET="3.6m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD2" OFFSET="1.8m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="SOLID" OFFSET="0m" WIDTH="20cm" COLOR="#FFFFFF" />
      <NOTRAFFICLANE OFFSET="-1.5m" WIDTH="3m" COLOR="RGB(0,128,0)" />
    </ROADLAYOUT>
    
    <ROADLAYOUT NAME="1LaneHighway" ROADTYPE="HIGHWAY">
      <NOTRAFFICLANE OFFSET="4.9m" WIDTH="3m" COLOR="RGB(0,128,0)" />
      <STRIPE TYPE="SOLID" OFFSET="3.6m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD1" OFFSET="1.8m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="SOLID" OFFSET="0m" WIDTH="20cm" COLOR="#FFFFFF" />
      <NOTRAFFICLANE OFFSET="-1.5m" WIDTH="3m" COLOR="RGB(0,128,0)" />
    </ROADLAYOUT>
    
    <ROADLAYOUT NAME="1LaneLocal" ROADTYPE="LOCAL">
      <NOTRAFFICLANE OFFSET="4.9m" WIDTH="3m" COLOR="RGB(0,128,0)" />
      <STRIPE TYPE="SOLID" OFFSET="3.6m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD1" OFFSET="1.8m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="SOLID" OFFSET="0m" WIDTH="20cm" COLOR="#FFFFFF" />
      <NOTRAFFICLANE OFFSET="-1.5m" WIDTH="3m" COLOR="RGB(0,128,0)" />
    </ROADLAYOUT>

  </DEFINITIONS>

  <NODE NAME="A" COORDINATE="(0, 0)" DIRECTION="0 deg" />
  <NODE NAME="B" COORDINATE="(1500, 0)" DIRECTION="0 deg" />
  <NODE NAME="C" COORDINATE="(1565, 0)" DIRECTION="0 deg" />
  <NODE NAME="D" COORDINATE="(1570, 0)" DIRECTION="0 deg" />
  <NODE NAME="E" COORDINATE="(3000, 0)" DIRECTION="0 deg" />
  <NODE NAME="F" COORDINATE="(1260, -40)" DIRECTION="0 deg" />
  <NODE NAME="F2" COORDINATE="(1280, -40)" DIRECTION="0 deg" />
  <NODE NAME="G" COORDINATE="(3000, 30)" DIRECTION="0 deg" />

  <LINK NAME="AB" NODESTART="A" NODEEND="B" ROADLAYOUT="3LaneHighway">
    <STRAIGHT />
  </LINK>
  
  <LINK NAME="BC" NODESTART="B" NODEEND="C" ROADLAYOUT="4LaneHighway">
    <STRAIGHT />
  </LINK>
  
  <LINK NAME="CD" NODESTART="C" NODEEND="D" ROADLAYOUT="3LaneHighway">
    <STRAIGHT />
  </LINK>
  
  <LINK NAME="DE" NODESTART="D" NODEEND="E" ROADLAYOUT="2LaneHighway">
    <STRAIGHT />
    <SINK POSITION="END-50m" LANE="FORWARD1" />
    <SINK POSITION="END-50m" LANE="FORWARD2" />
  </LINK>
  
  <LINK NAME="FF2" NODESTART="F" NODEEND="F2" ROADLAYOUT="1LaneLocal">
    <STRAIGHT />
  </LINK>
  
  <LINK NAME="F2B" NODESTART="F2" NODEEND="B" ROADLAYOUT="1LaneHighway" OFFSETEND="-3.6m">
    <BEZIER />
  </LINK>
  
  <LINK NAME="DG" NODESTART="D" NODEEND="G" ROADLAYOUT="1LaneHighway" OFFSETSTART="7.2m">
    <BEZIER />
    <SINK POSITION="END-50m" LANE="FORWARD1" />
  </LINK>

</NETWORK>