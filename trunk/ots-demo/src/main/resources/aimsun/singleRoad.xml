<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE xml>
<NETWORK xmlns="http://www.opentrafficsim.org/ots" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opentrafficsim.org/ots http://www.opentrafficsim.org/docs/current/ots-network.xsd">

  <DEFINITIONS>

    <GLOBAL />

    <GTUTYPE NAME="CAR" />
    
    <GTU NAME="CAR" GTUTYPE="CAR" LENGTH="UNIF(4,7) m" WIDTH="UNIF(1.7, 2) m" MAXSPEED="CONST(120) km/h" />

    <ROADTYPE NAME="HIGHWAY" DEFAULTLANEWIDTH="3.6m" DEFAULTLANEKEEPING="KEEPRIGHT"
      DEFAULTOVERTAKING="LEFTSET([CAR, TRUCK] OVERTAKE [ALL]) RIGHTSPEED(40 km/h)">
      <SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="120km/h" />
    </ROADTYPE>

    <ROADLAYOUT NAME="2LaneHighway" ROADTYPE="HIGHWAY">
      <NOTRAFFICLANE OFFSET="8.5m" WIDTH="3m" COLOR="RGB(0,128,0)" />
      <STRIPE TYPE="SOLID" OFFSET="7.2m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD1" OFFSET="5.4m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="DASHED" OFFSET="3.6m" WIDTH="20cm" COLOR="#FFFFFF" />
      <LANE NAME="FORWARD2" OFFSET="1.8m" WIDTH="3.6m" DIRECTION="FORWARD" />
      <STRIPE TYPE="SOLID" OFFSET="0m" WIDTH="20cm" COLOR="#FFFFFF" />
      <NOTRAFFICLANE OFFSET="-1.5m" WIDTH="3m" COLOR="RGB(0,128,0)" />
    </ROADLAYOUT>

  </DEFINITIONS>

  <NODE NAME="A" COORDINATE="(0, 0)" ANGLE="0 deg" />
  <NODE NAME="B" COORDINATE="(1500, 0)" ANGLE="0 deg" />

  <ROUTE NAME="AB" NODELIST="A B" />

  <LINK NAME="AB" NODESTART="A" NODEEND="B" ROADLAYOUT="2LaneHighway">
    <STRAIGHT />
    <GENERATOR LANE="FORWARD1" POSITION="10m" IAT="EXPO(5) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR" ROUTE="AB"
      GTUCOLORER="SWITCHABLE" />
    <GENERATOR LANE="FORWARD2" POSITION="10m" IAT="EXPO(5) s" INITIALSPEED="CONST(0.0) km/h" GTU="CAR" ROUTE="AB"
      GTUCOLORER="SWITCHABLE" />
    <SINK POSITION="END-50m" LANE="FORWARD1" />
    <SINK POSITION="END-50m" LANE="FORWARD2" />
  </LINK>

</NETWORK>