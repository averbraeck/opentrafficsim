<?xml version="1.0" encoding="UTF-8" ?>
<OTS xmlns="http://www.opentrafficsim.org/ots" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opentrafficsim.org/ots https://opentrafficsim.org/docs/xsd/1.02.01/ots.xsd"
  xmlns:xi="http://www.w3.org/2001/XInclude">

  <DEFINITIONS>
    <xi:include href="https://opentrafficsim.org/docs/xsd/1.02.01/defaults/default_gtutypes.xml" />
    <xi:include href="https://opentrafficsim.org/docs/xsd/1.02.01/defaults/default_linktypes.xml" />
    <xi:include href="https://opentrafficsim.org/docs/xsd/1.02.01/defaults/default_lanetypes.xml" />

    <GTUTEMPLATES>
      <GTUTEMPLATE DEFAULT="true" GTUTYPE="CAR" ID="CAR">
        <LENGTHDIST LENGTHUNIT="m">
          <UNIFORM MIN="3.0" MAX="4.0" />
        </LENGTHDIST>
        <WIDTHDIST LENGTHUNIT="m">
          <UNIFORM MIN="1.7" MAX="2.0" />
        </WIDTHDIST>
        <MAXSPEEDDIST SPEEDUNIT="km/h">
          <CONSTANT C="120" />
        </MAXSPEEDDIST>
      </GTUTEMPLATE>
    </GTUTEMPLATES>

    <LINKTYPES>
      <LINKTYPE ID="URBAN">
        <COMPATIBILITY GTUTYPE="VEHICLE" DIRECTION="FORWARD" />
        <SPEEDLIMIT GTUTYPE="CAR" LEGALSPEEDLIMIT="50km/h" />
      </LINKTYPE>
    </LINKTYPES>

    <LANETYPES>
      <LANETYPE ID="URBAN">
        <COMPATIBILITY GTUTYPE="VEHICLE" DIRECTION="FORWARD" />
      </LANETYPE>
    </LANETYPES>

    <ROADLAYOUTS>
      <ROADLAYOUT ID="2LANELC" LINKTYPE="URBAN">
        <SHOULDER>
          <CENTEROFFSET>-5m</CENTEROFFSET>
          <WIDTH>3m</WIDTH>
        </SHOULDER>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>-3.5m</CENTEROFFSET>
        </STRIPE>
        <LANE ID="RIGHT" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>-1.75m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="DASHED">
          <CENTEROFFSET>0.0m</CENTEROFFSET>
        </STRIPE>
        <LANE ID="LEFT" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>1.75m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>3.5m</CENTEROFFSET>
        </STRIPE>
        <SHOULDER>
          <CENTEROFFSET>5m</CENTEROFFSET>
          <WIDTH>3m</WIDTH>
        </SHOULDER>
      </ROADLAYOUT>

      <ROADLAYOUT ID="3LANELC" LINKTYPE="URBAN">
        <SHOULDER>
          <CENTEROFFSET>-5m</CENTEROFFSET>
          <WIDTH>3m</WIDTH>
        </SHOULDER>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>-3.5m</CENTEROFFSET>
        </STRIPE>
        <LANE ID="RIGHT" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>-1.75m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="DASHED">
          <CENTEROFFSET>0.0m</CENTEROFFSET>
        </STRIPE>
        <LANE ID="LEFT" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>1.75m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>3.5m</CENTEROFFSET>
        </STRIPE>
        <SHOULDER>
          <CENTEROFFSET>5m</CENTEROFFSET>
          <WIDTH>3m</WIDTH>
        </SHOULDER>
      </ROADLAYOUT>

      <ROADLAYOUT ID="2LANE" LINKTYPE="URBAN">
        <SHOULDER>
          <CENTEROFFSET>-5m</CENTEROFFSET>
          <WIDTH>3m</WIDTH>
        </SHOULDER>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>-3.5m</CENTEROFFSET>
        </STRIPE>
        <LANE ID="RIGHT" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>-1.75m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>0m</CENTEROFFSET>
        </STRIPE>
        <LANE ID="LEFT" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>1.75m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>3.5m</CENTEROFFSET>
        </STRIPE>
        <SHOULDER>
          <CENTEROFFSET>5m</CENTEROFFSET>
          <WIDTH>3m</WIDTH>
        </SHOULDER>
      </ROADLAYOUT>

      <ROADLAYOUT ID="1LEFT" LINKTYPE="URBAN">
        <SHOULDER>
          <CENTEROFFSET>-5m</CENTEROFFSET>
          <WIDTH>3m</WIDTH>
        </SHOULDER>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>-3.5m</CENTEROFFSET>
        </STRIPE>
        <LANE ID="LANE" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>-1.75m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>0m</CENTEROFFSET>
        </STRIPE>
        <SHOULDER>
          <CENTEROFFSET>1.5m</CENTEROFFSET>
          <WIDTH>3m</WIDTH>
        </SHOULDER>
      </ROADLAYOUT>

      <ROADLAYOUT ID="1RIGHT" LINKTYPE="URBAN">
        <SHOULDER>
          <CENTEROFFSET>-1.5m</CENTEROFFSET>
          <WIDTH>3m</WIDTH>
        </SHOULDER>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>0m</CENTEROFFSET>
        </STRIPE>
        <LANE ID="LANE" DESIGNDIRECTION="true" LANETYPE="URBAN">
          <CENTEROFFSET>1.75m</CENTEROFFSET>
          <WIDTH>3.5m</WIDTH>
        </LANE>
        <STRIPE TYPE="SOLID">
          <CENTEROFFSET>3.5m</CENTEROFFSET>
        </STRIPE>
        <SHOULDER>
          <CENTEROFFSET>5m</CENTEROFFSET>
          <WIDTH>3m</WIDTH>
        </SHOULDER>
      </ROADLAYOUT>
    </ROADLAYOUTS>
  </DEFINITIONS>

  <NETWORK>

    <NODE ID="E" COORDINATE="(300,0)" />
    <NODE ID="N" COORDINATE="(0,300)" />
    <NODE ID="W" COORDINATE="(-300,0)" />
    <NODE ID="S" COORDINATE="(0,-300)" />
    <NODE ID="EP" COORDINATE="(100,0)" />
    <NODE ID="NP" COORDINATE="(0,100)" />
    <NODE ID="WP" COORDINATE="(-100,0)" DIRECTION="0 deg"/>

    <LINK ID="EEP" NODESTART="E" NODEEND="EP" TYPE="URBAN" OFFSETSTART="-5.25m" OFFSETEND="-5.25m">
      <STRAIGHT />
      <DEFINEDLAYOUT>1RIGHT</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="EPEENTER" NODESTART="EP" NODEEND="EENTER" TYPE="URBAN" OFFSETSTART="-5.25m">
      <STRAIGHT />
      <DEFINEDLAYOUT>2LANELC</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="EEXITE" NODESTART="EEXIT" NODEEND="E" TYPE="URBAN" OFFSETEND="-1.75m">
      <STRAIGHT />
      <DEFINEDLAYOUT>1LEFT</DEFINEDLAYOUT>
    </LINK>

    <LINK ID="NNENTER" NODESTART="N" NODEEND="NENTER" TYPE="URBAN" OFFSETSTART="-5.25m">
      <STRAIGHT />
      <DEFINEDLAYOUT>2LANELC</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="NEXITNP" NODESTART="NEXIT" NODEEND="NP" TYPE="URBAN" OFFSETEND="-5.25m">
      <STRAIGHT />
      <DEFINEDLAYOUT>2LANELC</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="NPN" NODESTART="NP" NODEEND="N" TYPE="URBAN" OFFSETSTART="-5.25m" OFFSETEND="-5.25m">
      <STRAIGHT />
      <DEFINEDLAYOUT>1RIGHT</DEFINEDLAYOUT>
    </LINK>

    <LINK ID="WWP" NODESTART="W" NODEEND="WP" TYPE="URBAN" OFFSETSTART="-5.25m" OFFSETEND="-5.25m">
      <STRAIGHT />
      <DEFINEDLAYOUT>1RIGHT</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="WPWENTER" NODESTART="WP" NODEEND="WENTER" TYPE="URBAN" OFFSETSTART="-5.25m">
      <STRAIGHT />
      <DEFINEDLAYOUT>2LANELC</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="WEXITW" NODESTART="WEXIT" NODEEND="W" TYPE="URBAN" OFFSETEND="-1.75m">
      <STRAIGHT />
      <DEFINEDLAYOUT>1LEFT</DEFINEDLAYOUT>
    </LINK>

    <LINK ID="SSENTER" NODESTART="S" NODEEND="SENTER" TYPE="URBAN" OFFSETSTART="-5.25m">
      <STRAIGHT />
      <DEFINEDLAYOUT>2LANELC</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="SEXITS" NODESTART="SEXIT" NODEEND="S" TYPE="URBAN" OFFSETEND="-5.25m">
      <STRAIGHT />
      <DEFINEDLAYOUT>2LANELC</DEFINEDLAYOUT>
    </LINK>

    <!-- Roundabout itself, generated code -->
    <NODE ID="EENTER" COORDINATE="(44.0000,5.2500)" DIRECTION="180.0000 deg" />
    <NODE ID="EEXIT" COORDINATE="(44.0000,-1.7500)" DIRECTION="360.0000 deg" />
    <NODE ID="EA" COORDINATE="(15.9460,12.0808)" DIRECTION="122.9377 deg" />
    <NODE ID="EB" COORDINATE="(10.3308,17.6960)" DIRECTION="147.0623 deg" />
    <LINK ID="EENTEREA" NODESTART="EENTER" NODEEND="EA" TYPE="URBAN">
      <BEZIER SHAPE="1.1" WEIGHTED="true" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="EAEB" NODESTART="EA" NODEEND="EB" TYPE="URBAN">
      <ARC RADIUS="19.0000 m" DIRECTION="L" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="EBNA" NODESTART="EB" NODEEND="NA" TYPE="URBAN" OFFSETEND="-3.5m" PRIORITY="PRIORITY">
      <BEZIER SHAPE="0.75" />
      <DEFINEDLAYOUT>1RIGHT</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="EBNEXIT" NODESTART="EB" NODEEND="NEXIT" TYPE="URBAN">
      <BEZIER SHAPE="1.1" WEIGHTED="true" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <NODE ID="NENTER" COORDINATE="(-5.2500,44.0000)" DIRECTION="270.0000 deg" />
    <NODE ID="NEXIT" COORDINATE="(5.2500,44.0000)" DIRECTION="90.0000 deg" />
    <NODE ID="NA" COORDINATE="(-10.3308,14.1960)" DIRECTION="212.9377 deg" />
    <NODE ID="NB" COORDINATE="(-15.9460,8.5808)" DIRECTION="237.0623 deg" />
    <LINK ID="NENTERNA" NODESTART="NENTER" NODEEND="NA" TYPE="URBAN">
      <BEZIER SHAPE="1.1" WEIGHTED="true" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="NANB" NODESTART="NA" NODEEND="NB" TYPE="URBAN">
      <ARC RADIUS="19.0000 m" DIRECTION="L" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="NBWA" NODESTART="NB" NODEEND="WA" TYPE="URBAN" PRIORITY="PRIORITY">
      <ARC RADIUS="19.0000 m" DIRECTION="L" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="NBWEXIT" NODESTART="NB" NODEEND="WEXIT" TYPE="URBAN">
      <BEZIER SHAPE="1.1" WEIGHTED="true" />
      <DEFINEDLAYOUT>1LEFT</DEFINEDLAYOUT>
    </LINK>
    <NODE ID="WENTER" COORDINATE="(-44.0000,-5.2500)" DIRECTION="0 deg" />
    <NODE ID="WEXIT" COORDINATE="(-44.0000,1.7500)" DIRECTION="180.0000 deg" />
    <NODE ID="WA" COORDINATE="(-15.9460,-12.0808)" DIRECTION="302.9377 deg" />
    <NODE ID="WB" COORDINATE="(-10.3308,-17.6960)" DIRECTION="327.0623 deg" />
    <LINK ID="WENTERWA" NODESTART="WENTER" NODEEND="WA" TYPE="URBAN">
      <BEZIER SHAPE="1.1" WEIGHTED="true" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="WAWB" NODESTART="WA" NODEEND="WB" TYPE="URBAN">
      <ARC RADIUS="19.0000 m" DIRECTION="L" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="WBSA" NODESTART="WB" NODEEND="SA" TYPE="URBAN" OFFSETEND="-3.5m" PRIORITY="PRIORITY">
      <BEZIER SHAPE="0.75" />
      <DEFINEDLAYOUT>1RIGHT</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="WBSEXIT" NODESTART="WB" NODEEND="SEXIT" TYPE="URBAN">
      <BEZIER SHAPE="1.1" WEIGHTED="true" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <NODE ID="SENTER" COORDINATE="(5.2500,-44.0000)" DIRECTION="90.0000 deg" />
    <NODE ID="SEXIT" COORDINATE="(-5.2500,-44.0000)" DIRECTION="270.0000 deg" />
    <NODE ID="SA" COORDINATE="(10.3308,-14.1960)" DIRECTION="392.9377 deg" />
    <NODE ID="SB" COORDINATE="(15.9460,-8.5808)" DIRECTION="417.0623 deg" />
    <LINK ID="SENTERSA" NODESTART="SENTER" NODEEND="SA" TYPE="URBAN">
      <BEZIER SHAPE="1.1" WEIGHTED="true" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="SASB" NODESTART="SA" NODEEND="SB" TYPE="URBAN">
      <ARC RADIUS="19.0000 m" DIRECTION="L" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="SBEA" NODESTART="SB" NODEEND="EA" TYPE="URBAN" PRIORITY="PRIORITY">
      <ARC RADIUS="19.0000 m" DIRECTION="L" />
      <DEFINEDLAYOUT>2LANE</DEFINEDLAYOUT>
    </LINK>
    <LINK ID="SBEEXIT" NODESTART="SB" NODEEND="EEXIT" TYPE="URBAN">
      <BEZIER SHAPE="1.1" WEIGHTED="true" />
      <DEFINEDLAYOUT>1LEFT</DEFINEDLAYOUT>
    </LINK>

  </NETWORK>

  <NETWORKDEMAND>

    <SHORTESTROUTE ID="EN" GTUTYPE="CAR">
      <FROM NODE="E" />
      <TO NODE="N" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>
    <SHORTESTROUTE ID="EW" GTUTYPE="CAR">
      <FROM NODE="E" />
      <TO NODE="W" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>
    <SHORTESTROUTE ID="ES" GTUTYPE="CAR">
      <FROM NODE="E" />
      <TO NODE="S" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>

    <SHORTESTROUTE ID="NW" GTUTYPE="CAR">
      <FROM NODE="N" />
      <TO NODE="W" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>
    <SHORTESTROUTE ID="NS" GTUTYPE="CAR">
      <FROM NODE="N" />
      <TO NODE="S" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>
    <SHORTESTROUTE ID="NE" GTUTYPE="CAR">
      <FROM NODE="N" />
      <TO NODE="E" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>

    <SHORTESTROUTE ID="WS" GTUTYPE="CAR">
      <FROM NODE="W" />
      <TO NODE="S" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>
    <SHORTESTROUTE ID="WE" GTUTYPE="CAR">
      <FROM NODE="W" />
      <TO NODE="E" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>
    <SHORTESTROUTE ID="WN" GTUTYPE="CAR">
      <FROM NODE="W" />
      <TO NODE="N" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>

    <SHORTESTROUTE ID="SE" GTUTYPE="CAR">
      <FROM NODE="S" />
      <TO NODE="E" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>
    <SHORTESTROUTE ID="SN" GTUTYPE="CAR">
      <FROM NODE="S" />
      <TO NODE="N" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>
    <SHORTESTROUTE ID="SW" GTUTYPE="CAR">
      <FROM NODE="S" />
      <TO NODE="W" />
      <DISTANCECOST>1/m</DISTANCECOST>
    </SHORTESTROUTE>

    <SHORTESTROUTEMIX ID="E" GTUTYPE="CAR">
      <SHORTESTROUTE WEIGHT="0.40" ID="EN" />
      <SHORTESTROUTE WEIGHT="0.20" ID="EW" />
      <SHORTESTROUTE WEIGHT="0.40" ID="ES" />
    </SHORTESTROUTEMIX>

    <SHORTESTROUTEMIX ID="N" GTUTYPE="CAR">
      <SHORTESTROUTE WEIGHT="0.25" ID="NW" />
      <SHORTESTROUTE WEIGHT="0.50" ID="NS" />
      <SHORTESTROUTE WEIGHT="0.25" ID="NE" />
    </SHORTESTROUTEMIX>

    <SHORTESTROUTEMIX ID="W" GTUTYPE="CAR">
      <SHORTESTROUTE WEIGHT="0.4" ID="WS" />
      <SHORTESTROUTE WEIGHT="0.2" ID="WE" />
      <SHORTESTROUTE WEIGHT="0.4" ID="WN" />
    </SHORTESTROUTEMIX>

    <SHORTESTROUTEMIX ID="S" GTUTYPE="CAR">
      <SHORTESTROUTE WEIGHT="0.25" ID="SE" />
      <SHORTESTROUTE WEIGHT="0.50" ID="SN" />
      <SHORTESTROUTE WEIGHT="0.25" ID="SW" />
    </SHORTESTROUTEMIX>

    <GENERATOR LINK="EEP" LANE="LANE" POSITION="10m" DIRECTION="DIR_PLUS">
      <GTUTEMPLATE>CAR</GTUTEMPLATE>
      <SHORTESTROUTEMIX>E</SHORTESTROUTEMIX>
      <FREQUENCY>25/min</FREQUENCY>
      <ROOMCHECKER>CF</ROOMCHECKER>
    </GENERATOR>

    <GENERATOR LINK="NNENTER" LANE="LEFT" POSITION="10m" DIRECTION="DIR_PLUS">
      <GTUTEMPLATE>CAR</GTUTEMPLATE>
      <SHORTESTROUTEMIX>N</SHORTESTROUTEMIX>
      <FREQUENCY>12.5/min</FREQUENCY>
      <ROOMCHECKER>CF</ROOMCHECKER>
    </GENERATOR>
      
    <GENERATOR LINK="NNENTER" LANE="RIGHT" POSITION="10m" DIRECTION="DIR_PLUS">
      <GTUTEMPLATE>CAR</GTUTEMPLATE>
      <SHORTESTROUTEMIX>N</SHORTESTROUTEMIX>
      <FREQUENCY>12.5/min</FREQUENCY>
      <ROOMCHECKER>CF</ROOMCHECKER>
    </GENERATOR>
      
    <GENERATOR LINK="WWP" LANE="LANE" POSITION="10m" DIRECTION="DIR_PLUS">
      <GTUTEMPLATE>CAR</GTUTEMPLATE>
      <SHORTESTROUTEMIX>W</SHORTESTROUTEMIX>
      <FREQUENCY>25/min</FREQUENCY>
      <ROOMCHECKER>CF</ROOMCHECKER>
    </GENERATOR>

    <GENERATOR LINK="SSENTER" LANE="LEFT" POSITION="10m" DIRECTION="DIR_PLUS">
      <GTUTEMPLATE>CAR</GTUTEMPLATE>
      <SHORTESTROUTEMIX>S</SHORTESTROUTEMIX>
      <FREQUENCY>12.5/min</FREQUENCY>
      <ROOMCHECKER>CF</ROOMCHECKER>
    </GENERATOR>
      
    <GENERATOR LINK="SSENTER" LANE="RIGHT" POSITION="10m" DIRECTION="DIR_PLUS">
      <GTUTEMPLATE>CAR</GTUTEMPLATE>
      <SHORTESTROUTEMIX>S</SHORTESTROUTEMIX>
      <FREQUENCY>12.5/min</FREQUENCY>
      <ROOMCHECKER>CF</ROOMCHECKER>
    </GENERATOR>

    <SINK LINK="EEXITE" LANE="LANE" POSITION="END-20m" DIRECTION="DIR_PLUS" />
    <SINK LINK="NPN" LANE="LANE" POSITION="END-20m" DIRECTION="DIR_PLUS" />
    <SINK LINK="WEXITW" LANE="LANE" POSITION="END-20m" DIRECTION="DIR_PLUS" />
    <SINK LINK="SEXITS" LANE="LEFT" POSITION="END-20m" DIRECTION="DIR_PLUS" />
    <SINK LINK="SEXITS" LANE="RIGHT" POSITION="END-20m" DIRECTION="DIR_PLUS" />

  </NETWORKDEMAND>

  <SCENARIO></SCENARIO>

  <RUN>
    <RUNLENGTH>1h</RUNLENGTH>
    <RANDOMSTREAMS>
      <RANDOMSTREAM ID="default">
        <REPLICATION ID="1" SEED="1" />
      </RANDOMSTREAM>
      <RANDOMSTREAM ID="generation">
        <REPLICATION ID="1" SEED="1" />
      </RANDOMSTREAM>
    </RANDOMSTREAMS>
  </RUN>

  <ANIMATION>
    <DEFAULTS>
      <SHOULDER COLOR="RGB(0,128,0)" />
    </DEFAULTS>
  </ANIMATION>

</OTS>