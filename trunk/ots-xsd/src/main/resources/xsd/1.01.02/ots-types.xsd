  <!-- ================================================================================================== -->
  <!-- ================================================= TYPES ========================================== -->
  <!-- ================================================================================================== -->

  <xsd:simpleType name="CLASSNAMETYPE">
    <xsd:annotation>
      <xsd:documentation source="description">
        Fully specified classname including the package.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([A-Za-z_0-9$]+\.)*[A-Za-z_0-9$]+"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="PERLENGTHTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d+\.?\d*\s*\/\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- Allows patterns like BEGIN | END | END-10m | 25m | 50% -->
  <xsd:simpleType name="LENGTHBEGINENDTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern
        value="BEGIN|END|END\-(\d+\.?\d*\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft))|(\
){0}(\d+\.?\d*\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft)|(\
){0}((\d(\.\d*)?)|(\d\d(\.\d*)?)|(100(\.\0*)?))\s*\%)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="FRACTIONTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(0?\.\d*|1\.0*|1|0)|((\d(\.\d*)?)|(\d\d(\.\d*)?)|(100(\.\0*)?))\s*\%"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="POSITIVEDOUBLE">
    <xsd:restriction base="xsd:decimal">
      <xsd:minExclusive value="0" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="SIGNEDLENGTHTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[+-]?\d+\.?\d*\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="VEHICLEINTENSITYTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d+\.?\d*\s*veh"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="VEHICLEFREQUENCYTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d+\.?\d*\s*veh\s*\/\s*(ms|s|min|h|day|wk)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="COORDINATETYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\(\s*[+-]?\d+\.?\d*\s*,\s*[+-]?\d+\.?\d*(\s*,\s*[+-]?\d+\.?\d*)?\s*\)">
      </xsd:pattern>
      <xsd:whiteSpace value="collapse" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="COORDINATENOSPACETYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\([+-]?\d+\.?\d*,[+-]?\d+\.?\d*(,[+-]?\d+\.?\d*)?\)">
      </xsd:pattern>
      <xsd:whiteSpace value="collapse" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="COORDINATELISTTYPE">
    <xsd:list itemType="COORDINATENOSPACETYPE" />
  </xsd:simpleType>

  <xsd:simpleType name="LEFTRIGHTTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="L|LEFT|R|RIGHT|CLOCKWISE|COUNTERCLOCKWISE"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="DISTANCECOSTTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d+\.?\d*\s*\/\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="TIMECOSTTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d+\.?\d*\s*\/\s*(ms|s|min|h|day|wk)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="LANEKEEPINGTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="KEEPLEFT|KEEPRIGHT|KEEPLANE"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="PRIORITYTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="PRIORITY|NONE|TURN_ON_RED|YIELD|STOP|ALL_STOP|BUS_STOP"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="GTUPOSITIONTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="FRONT|REAR|REFERENCE"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="STRIPETYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="SOLID|\||DASHED|\:|BLOCKED|DOUBLE|\|\||RIGHTTOLEFT|\|\:|LEFTTORIGHT|\:\|"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="DRIVINGDIRECTIONTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="FORWARD|BACKWARD|BOTH|NONE"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="GTUDIRECTIONTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="DIR_PLUS|DIR_MINUS"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="INTERPOLATIONTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="STEPWISE|LINEAR"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="COLORTYPE">
    <xsd:restriction base="xsd:string">
      <xsd:pattern
        value="(\
{0})WHITE|LIGHT_GRAY|GRAY|DARK_GRAY|BLACK|RED|PINK|ORANGE|YELLOW|GREEN|MAGENTA|CYAN|BLUE(\
{0})|\#[(0-9)|(A-F)]{6}(\
{0})|RGB\(0*([1-9]?\d|1\d\d|2[0-4]\d|25[0-5])\s*\,\s*(\
{0})0*([1-9]?\d|1\d\d|2[0-4]\d|25[0-5])\s*\,\s*(\
{0})0*([1-9]?\d|1\d\d|2[0-4]\d|25[0-5])\)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="PARAMETERTYPE">
    <xsd:attribute name="TYPE" use="required">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:pattern
            value="(\
{0})CLASS|STRING|DOUBLE|INTEGER|(\
{0})LENGTH|POSITION|DURATION|TIME|SPEED|ACCELERATION|FREQUENCY|LINEARDENSITY|(\
{0})FRACTION" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="VALUE" type="xsd:string" use="required" />
    <xsd:attribute name="ID" type="xsd:string" use="required" />
    <xsd:attribute name="CLASS" type="CLASSNAMETYPE" use="optional" />
  </xsd:complexType>

  <!-- ========================================================================================= -->
  <!-- ==================================== DISTRIBUTION TYPES ================================= -->
  <!-- ========================================================================================= -->

  <xsd:simpleType name="DOUBLEPOSITIVEZERO">
    <xsd:restriction base="xsd:double">
      <xsd:minInclusive value="0.0"></xsd:minInclusive>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="DOUBLEPOSITIVE">
    <xsd:restriction base="xsd:double">
      <xsd:minExclusive value="0.0"></xsd:minExclusive>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="CONTDISTTYPE">
    <xsd:choice>
      <xsd:element name="CONSTANT">
        <xsd:complexType>
          <xsd:attribute name="C" type="xsd:double" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="EXPONENTIAL">
        <xsd:complexType>
          <xsd:attribute name="LAMBDA" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="TRIANGULAR">
        <xsd:complexType>
          <xsd:attribute name="MIN" type="xsd:double" use="required" />
          <xsd:attribute name="MODE" type="xsd:double" use="required" />
          <xsd:attribute name="MAX" type="xsd:double" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="NORMAL">
        <xsd:complexType>
          <xsd:attribute name="MU" type="xsd:double" use="required" />
          <xsd:attribute name="SIGMA" type="DOUBLEPOSITIVEZERO" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="BETA">
        <xsd:complexType>
          <xsd:attribute name="ALPHA1" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="ALPHA2" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ERLANG">
        <xsd:complexType>
          <xsd:attribute name="MEAN" type="xsd:double" use="required" />
          <xsd:attribute name="K" type="xsd:positiveInteger" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="GAMMA">
        <xsd:complexType>
          <xsd:attribute name="ALPHA" type="xsd:double" use="required" />
          <xsd:attribute name="BETA" type="xsd:double" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="LOGNORMAL">
        <xsd:complexType>
          <xsd:attribute name="MU" type="xsd:double" use="required" />
          <xsd:attribute name="SIGMA" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="PEARSON5">
        <xsd:complexType>
          <xsd:attribute name="ALPHA" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="BETA" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="PEARSON6">
        <xsd:complexType>
          <xsd:attribute name="ALPHA1" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="ALPHA2" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="BETA" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="UNIFORM">
        <xsd:complexType>
          <xsd:attribute name="MIN" type="xsd:double" use="required" />
          <xsd:attribute name="MAX" type="xsd:double" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="WEIBULL">
        <xsd:complexType>
          <xsd:attribute name="ALPHA" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="BETA" type="DOUBLEPOSITIVE" use="required" />
          <xsd:attribute name="RANDOMSTREAM" type="xsd:string" use="optional" default="default" />
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>

  <xsd:complexType name="LENGTHDISTTYPE">
    <xsd:complexContent>
      <xsd:extension base="CONTDISTTYPE">
        <xsd:attribute name="LENGTHUNIT" type="LENGTHUNITTYPE" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="POSITIONDISTTYPE">
    <xsd:complexContent>
      <xsd:extension base="CONTDISTTYPE">
        <xsd:attribute name="POSITIONUNIT" type="POSITIONUNITTYPE" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="SPEEDDISTTYPE">
    <xsd:complexContent>
      <xsd:extension base="CONTDISTTYPE">
        <xsd:attribute name="SPEEDUNIT" type="SPEEDUNITTYPE" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="ACCELERATIONDISTTYPE">
    <xsd:complexContent>
      <xsd:extension base="CONTDISTTYPE">
        <xsd:attribute name="ACCELERATIONUNIT" type="ACCELERATIONUNITTYPE" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="TIMEDISTTYPE">
    <xsd:complexContent>
      <xsd:extension base="CONTDISTTYPE">
        <xsd:attribute name="TIMEUNIT" type="TIMEUNITTYPE" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="DURATIONDISTTYPE">
    <xsd:complexContent>
      <xsd:extension base="CONTDISTTYPE">
        <xsd:attribute name="DURATIONUNIT" type="DURATIONUNITTYPE" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

</xsd:schema>