  <!-- ================================================================================================== -->
  <!-- ================================================= TYPES ========================================== -->
  <!-- ================================================================================================== -->

  <xsd:simpleType name="Expression">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}" />
    </xsd:restriction>
  </xsd:simpleType>
  
  <!-- Forces that when '{ }' is used, no '{' or '}' is used between them, otherwise no '{' at start and no '}' at end. -->
  <xsd:simpleType name="string">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|([^{](.*[^}])?)" />
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="MultiLineString">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|([^{](([\s\S])*[^}])?)" />
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="space">
    <xsd:union memberTypes="ots:Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="default" />
          <xsd:enumeration value="preserve" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>
  
  <xsd:simpleType name="boolean">
    <xsd:union memberTypes="ots:Expression xsd:boolean" />
  </xsd:simpleType>
  
  <xsd:simpleType name="int">
    <xsd:union memberTypes="ots:Expression xsd:int" />
  </xsd:simpleType>
  
  <xsd:simpleType name="integer">
    <xsd:union memberTypes="ots:Expression xsd:integer" />
  </xsd:simpleType>
  
  <xsd:simpleType name="positiveInteger">
    <xsd:union memberTypes="ots:Expression xsd:positiveInteger" />
  </xsd:simpleType>
  
  <xsd:simpleType name="long">
    <xsd:union memberTypes="ots:Expression xsd:long" />
  </xsd:simpleType>
  
  <xsd:simpleType name="double">
    <xsd:union memberTypes="ots:Expression xsd:double" />
  </xsd:simpleType>
  
  <xsd:simpleType name="float">
    <xsd:union memberTypes="ots:Expression xsd:float" />
  </xsd:simpleType>
  
  <xsd:simpleType name="anyURI">
    <xsd:union memberTypes="ots:Expression xsd:anyURI" />
  </xsd:simpleType>

  <xsd:simpleType name="IntAtLeastOne">
    <xsd:union memberTypes="ots:Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:positiveInteger">
          <xsd:minInclusive value="1"></xsd:minInclusive>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="DoublePositiveInclusive">
    <xsd:union memberTypes="ots:Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:double">
          <xsd:minInclusive value="0.0"></xsd:minInclusive>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="DoublePositive">
    <xsd:union memberTypes="ots:Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:double">
          <xsd:minExclusive value="0.0"></xsd:minExclusive>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="DoubleUnitInterval">
    <xsd:union memberTypes="ots:Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:double">
          <xsd:minInclusive value="0.0"></xsd:minInclusive>
          <xsd:maxInclusive value="1.0"></xsd:maxInclusive>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:complexType name="ModelIdReferralType">
    <xsd:attribute name="Id" type="xsd:string" />
    <xsd:attribute name="ModelId" type="ots:string" />
  </xsd:complexType>

  <xsd:simpleType name="ClassNameType">
    <xsd:annotation>
      <xsd:appinfo source="pattern">Provide a full class name with dot-notation.</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|(([A-Za-z_0-9$]+\.)*[A-Za-z_0-9$]+)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- Same as ClassNameType, just a different binding. -->
  <xsd:simpleType name="StaticFieldNameType">
    <xsd:annotation>
      <xsd:appinfo source="pattern">Provide a full field name with dot-notation.</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|(([A-Za-z_0-9$]+\.)*[A-Za-z_0-9$]+)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- Allows patterns like BEGIN | END | END-10m | 25m | 50% -->
  <xsd:simpleType name="LengthBeginEndType">
    <xsd:annotation>
      <xsd:appinfo source="pattern">Provide a value as BEGIN, END, END-10m, 25m, or 50%.</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|(BEGIN|END|END\-(\d+\.?\d*\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft))|(\
){0}(\d+\.?\d*\s*(mm|cm|dm|m|dam|hm|km|mi|yd|ft)|(\
){0}((\d(\.\d*)?)|(\d\d(\.\d*)?)|(100(\.0*)?))\s*\%))"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="FractionType">
    <xsd:annotation>
      <xsd:appinfo source="pattern">Provide a value as decimal number [0 .. 1] or percentage [0% .. 100%].</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|((0?\.\d*|1\.0*|1|0)|((\d(\.\d*)?)|(\d\d(\.\d*)?)|(100(\.0*)?))\s*\%)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="VehicleIntensityType">
    <xsd:annotation>
      <xsd:appinfo source="pattern">Provide a value as decimal number [0 .. 1] or percentage [0% .. 100%].</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|(\d+\.?\d*\s*veh)"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="VehicleFrequencyType">
    <xsd:annotation>
      <xsd:appinfo source="pattern">Provide a value as X.Xveh/{time unit}.</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|(\d+\.?\d*\s*veh\s*\/\s*(ms|s|min|h|day|wk))"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="CoordinateType">
    <xsd:annotation>
      <xsd:appinfo source="pattern">Provide a value as (X.X, X.X).</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|(\(\s*[+-]?\d+\.?\d*\s*,\s*[+-]?\d+\.?\d*\s*\))">
      </xsd:pattern>
      <xsd:whiteSpace value="collapse" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="CoordinateNoSpaceType">
    <xsd:annotation>
      <xsd:appinfo source="pattern">Provide a value as (X.X,X.X).</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|(\([+-]?\d+\.?\d*,[+-]?\d+\.?\d*\))">
      </xsd:pattern>
      <xsd:whiteSpace value="collapse" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ArcDirectionType">
    <xsd:union memberTypes="ots:Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="L" />
          <xsd:enumeration value="LEFT" />
          <xsd:enumeration value="R" />
          <xsd:enumeration value="RIGHT" />
          <xsd:enumeration value="CLOCKWISE" />
          <xsd:enumeration value="COUNTERCLOCKWISE" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>
  
  <xsd:complexType name="LaneLinkType">
