        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="StripeType">
    <xsd:union memberTypes="ots:Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="SOLID" />
          <xsd:enumeration value="|" />
          <xsd:enumeration value="DASHED" />
          <xsd:enumeration value=":" />
          <xsd:enumeration value="BLOCK" />
          <xsd:enumeration value="DOUBLE" />
          <xsd:enumeration value="||" />
          <xsd:enumeration value="LEFT" />
          <xsd:enumeration value="|:" />
          <xsd:enumeration value="RIGHT" />
          <xsd:enumeration value=":|" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="DrivingDirectionType">
    <xsd:union memberTypes="ots:Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="FORWARD" />
          <xsd:enumeration value="BACKWARD" />
          <xsd:enumeration value="BOTH" />
          <xsd:enumeration value="NONE" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="GtuDirectionType">
    <xsd:union memberTypes="ots:Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="DIR_PLUS" />
          <xsd:enumeration value="DIR_MINUS" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="InterpolationType">
    <xsd:union memberTypes="ots:Expression">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="STEPWISE" />
          <xsd:enumeration value="LINEAR" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:union>
  </xsd:simpleType>

  <xsd:simpleType name="ColorType">
    <xsd:annotation>
      <xsd:appinfo source="pattern">E.g. RED, #FF80AA, or RGB(255,0,80).</xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\{[^{}]+\}|((\
{0})WHITE|LIGHT_GRAY|GRAY|DARK_GRAY|BLACK|RED|PINK|ORANGE|YELLOW|GREEN|MAGENTA|CYAN|BLUE(\
{0})|\#[(0-9)|(A-F)]{6}(\
{0})|RGB\(0*([1-9]?\d|1\d\d|2[0-4]\d|25[0-5])\s*\,\s*(\
{0})0*([1-9]?\d|1\d\d|2[0-4]\d|25[0-5])\s*\,\s*(\
{0})0*([1-9]?\d|1\d\d|2[0-4]\d|25[0-5])\))"></xsd:pattern>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- This type does not used ots:XXX types as we do not allow expressions in input parameters themselves. -->
  <xsd:complexType name="InputParameterType">
    <xsd:attribute name="Type" use="required">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="CLASS" />
          <xsd:enumeration value="STRING" />
          <xsd:enumeration value="DOUBLE" />
          <xsd:enumeration value="INTEGER" />
          <xsd:enumeration value="LENGTH" />
          <xsd:enumeration value="POSITION" />
          <xsd:enumeration value="DURATION" />
          <xsd:enumeration value="TIME" />
          <xsd:enumeration value="SPEED" />
          <xsd:enumeration value="ACCELERATION" />
          <xsd:enumeration value="FREQUENCY" />
          <xsd:enumeration value="LINEARDENSITY" />
          <xsd:enumeration value="FRACTION" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="Value" type="xsd:string" use="required" />
    <xsd:attribute name="Id" type="xsd:string" use="required" />
    <xsd:attribute name="Class" type="ots:ClassNameType" use="optional" />
  </xsd:complexType>
  
  <xsd:complexType name="EmptyType" />
  
  <xsd:complexType name="RandomStreamSource">
    <xsd:choice>
      <xsd:element name="Default" type="ots:EmptyType" />
      <xsd:element name="Generation" type="ots:EmptyType" />
      <xsd:element name="Defined" type="ots:string" />
    </xsd:choice>
  </xsd:complexType>

  <!-- ========================================================================================= -->
  <!-- ==================================== DISTRIBUTION TYPES ================================= -->
  <!-- ========================================================================================= -->

  <xsd:complexType name="ConstantDistType">
    <xsd:sequence>
	  <xsd:choice>
	    <xsd:element name="Constant">
	      <xsd:complexType>
	        <xsd:attribute name="C" type="ots:double" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Exponential">
	      <xsd:complexType>
	        <xsd:attribute name="Lambda" type="ots:DoublePositive" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Triangular">
	      <xsd:complexType>
	        <xsd:attribute name="Min" type="ots:double" use="required" />
	        <xsd:attribute name="Mode" type="ots:double" use="required" />
	        <xsd:attribute name="Max" type="ots:double" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Normal">
	      <xsd:complexType>
	        <xsd:attribute name="Mu" type="ots:double" use="required" />
	        <xsd:attribute name="Sigma" type="ots:DoublePositiveInclusive" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="NormalTrunc">
	      <xsd:complexType>
	        <xsd:attribute name="Mu" type="ots:double" use="required" />
	        <xsd:attribute name="Sigma" type="ots:DoublePositiveInclusive" use="required" />
	        <xsd:attribute name="Min" type="ots:double" use="required" />
	        <xsd:attribute name="Max" type="ots:double" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Beta">
	      <xsd:complexType>
	        <xsd:attribute name="Alpha1" type="ots:DoublePositive" use="required" />
	        <xsd:attribute name="Alpha2" type="ots:DoublePositive" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Erlang">
	      <xsd:complexType>
	        <xsd:attribute name="Mean" type="ots:double" use="required" />
	        <xsd:attribute name="K" type="ots:positiveInteger" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Gamma">
	      <xsd:complexType>
	        <xsd:attribute name="Alpha" type="ots:double" use="required" />
	        <xsd:attribute name="Beta" type="ots:double" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="LogNormal">
	      <xsd:complexType>
	        <xsd:attribute name="Mu" type="ots:double" use="required" />
	        <xsd:attribute name="Sigma" type="ots:DoublePositive" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Pearson5">
	      <xsd:complexType>
	        <xsd:attribute name="Alpha" type="ots:DoublePositive" use="required" />
	        <xsd:attribute name="Beta" type="ots:DoublePositive" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Pearson6">
	      <xsd:complexType>
	        <xsd:attribute name="Alpha1" type="ots:DoublePositive" use="required" />
	        <xsd:attribute name="Alpha2" type="ots:DoublePositive" use="required" />
	        <xsd:attribute name="Beta" type="ots:DoublePositive" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Uniform">
	      <xsd:complexType>
	        <xsd:attribute name="Min" type="ots:double" use="required" />
	        <xsd:attribute name="Max" type="ots:double" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Weibull">
	      <xsd:complexType>
	        <xsd:attribute name="Alpha" type="ots:DoublePositive" use="required" />
	        <xsd:attribute name="Beta" type="ots:DoublePositive" use="required" />
	      </xsd:complexType>
	    </xsd:element>
      </xsd:choice>
      <xsd:element name="RandomStream" type="ots:RandomStreamSource" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DiscreteDistType">
    <xsd:sequence>
	  <xsd:choice>
	    <xsd:element name="Constant">
	      <xsd:complexType>
	        <xsd:attribute name="C" type="ots:long" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="BernoulliI">
	      <xsd:complexType>
	        <xsd:attribute name="P" type="ots:FractionType" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Binomial">
	      <xsd:complexType>
	        <xsd:attribute name="N" type="ots:positiveInteger" use="required" />
	        <xsd:attribute name="P" type="ots:FractionType" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Uniform">
	      <xsd:complexType>
	        <xsd:attribute name="Min" type="ots:int" use="required" />
	        <xsd:attribute name="Max" type="ots:int" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Geometric">
	      <xsd:complexType>
	        <xsd:attribute name="P" type="ots:FractionType" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="NegBinomial">
	      <xsd:complexType>
	        <xsd:attribute name="N" type="ots:positiveInteger" use="required" />
	        <xsd:attribute name="P" type="ots:FractionType" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	    <xsd:element name="Poisson">
	      <xsd:complexType>
	        <xsd:attribute name="Lambda" type="ots:DoublePositive" use="required" />
	      </xsd:complexType>
	    </xsd:element>
	  </xsd:choice>
	  <xsd:element name="RandomStream" type="ots:RandomStreamSource" minOccurs="0"/>
	</xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="LengthDistType">
