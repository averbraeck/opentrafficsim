<ots:LinkTypes xmlns:ots="http://www.opentrafficsim.org/ots"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opentrafficsim.org/ots ../ots-definitions.xsd">
  
  <ots:LinkType Id="NL.CONNECTOR" Default="true">
    <ots:Compatibility GtuType="NL.ROAD_USER" />
    <ots:Compatibility GtuType="NL.WATERWAY_USER" />
    <ots:Compatibility GtuType="NL.RAILWAY_USER" />
  </ots:LinkType>

  <ots:LinkType Id="NL.ROAD" Default="true">
    <ots:Compatibility GtuType="NL.ROAD_USER" />
  </ots:LinkType>

  <ots:LinkType Id="NL.FREEWAY" Parent="NL.ROAD" Default="true">
    <ots:Compatibility GtuType="NL.PEDESTRIAN" Compatible="false" />
    <ots:Compatibility GtuType="NL.BICYCLE" Compatible="false" />
  </ots:LinkType>
  
  <ots:LinkType Id="NL.HIGHWAY" Parent="NL.ROAD" Default="true">
    <ots:Compatibility GtuType="NL.PEDESTRIAN" Compatible="false" />
    <ots:Compatibility GtuType="NL.BICYCLE" Compatible="false" />
  </ots:LinkType>
  
  <ots:LinkType Id="NL.PROVINCIAL" Parent="NL.ROAD" Default="true">
    <ots:Compatibility GtuType="NL.PEDESTRIAN" Compatible="false" />
    <ots:Compatibility GtuType="NL.BICYCLE" Compatible="false" />
  </ots:LinkType>
  
  <ots:LinkType Id="NL.RURAL" Parent="NL.ROAD" Default="true" />
  
  <ots:LinkType Id="NL.URBAN" Parent="NL.ROAD" Default="true" />
  
  <ots:LinkType Id="NL.RESIDENTIAL" Parent="NL.ROAD" Default="true" />

  <ots:LinkType Id="NL.WATERWAY" Default="true">
    <ots:Compatibility GtuType="NL.WATERWAY_USER" />
  </ots:LinkType>

  <ots:LinkType Id="NL.RAILWAY" Default="true">
    <ots:Compatibility GtuType="NL.RAILWAY_USER" />
  </ots:LinkType>
  
</ots:LinkTypes>
